
<div *ngIf="segments">
    <h2><span jhiTranslate="agreeGatewayV1App.segments.detail.title">Segments</span> {{segments.id}}</h2>
    <hr>
    <jhi-alert-error></jhi-alert-error>
    <dl class="row-md jh-entity-details">
        <dt><span jhiTranslate="agreeGatewayV1App.segments.coaId">Coa Id</span></dt>
        <dd>
            <span>{{segments.coaId}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.segmentName">Segment Name</span></dt>
        <dd>
            <span>{{segments.segmentName}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.values">Values</span></dt>
        <dd>
            <span>{{segments.values}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.segmentLength">Segment Length</span></dt>
        <dd>
            <span>{{segments.segmentLength}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.createdBy">Created By</span></dt>
        <dd>
            <span>{{segments.createdBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.createdDate">Created Date</span></dt>
        <dd>
            <span>{{segments.createdDate | date:'medium'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.lastUpdatedBy">Last Updated By</span></dt>
        <dd>
            <span>{{segments.lastUpdatedBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.segments.lastUpdatedDate">Last Updated Date</span></dt>
        <dd>
            <span>{{segments.lastUpdatedDate | date:'medium'}}</span>
        </dd>
    </dl>

    <button type="submit"
            (click)="previousState()"
            class="btn btn-info">
        <span class="fa fa-arrow-left"></span>&nbsp;<span jhiTranslate="entity.action.back"> Back</span>
    </button>

    <button type="button"
            [routerLink]="['/', { outlets: { popup: 'segments/'+ segments.id + '/edit'} }]"
            replaceUrl="true"
            class="btn btn-primary">
        <span class="fa fa-pencil"></span>&nbsp;<span jhiTranslate="entity.action.edit"> Edit</span>
    </button>
</div>
