<form name="editForm" role="form" novalidate (ngSubmit)="save()" #editForm="ngForm">

    <div class="modal-header">
        <h4 class="modal-title" id="myAcctRuleConditionsLabel" jhiTranslate="agreeGatewayV1App.acctRuleConditions.home.createOrEditLabel">Create or edit a Acct Rule Conditions</h4>
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"
                (click)="clear()">&times;</button>
    </div>
    <div class="modal-body">
        <jhi-alert-error></jhi-alert-error>
        <div class="form-group" [hidden]="!acctRuleConditions.id">
            <label for="id" jhiTranslate="global.field.id">ID</label>
            <input type="text" class="form-control" id="id" name="id"
                   [(ngModel)]="acctRuleConditions.id" readonly />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.ruleId" for="field_ruleId">Rule Id</label>
            <input type="number" class="form-control" name="ruleId" id="field_ruleId"
                [(ngModel)]="acctRuleConditions.ruleId"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.ruleActionId" for="field_ruleActionId">Rule Action Id</label>
            <input type="number" class="form-control" name="ruleActionId" id="field_ruleActionId"
                [(ngModel)]="acctRuleConditions.ruleActionId"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.openBracket" for="field_openBracket">Open Bracket</label>
            <input type="text" class="form-control" name="openBracket" id="field_openBracket"
                [(ngModel)]="acctRuleConditions.openBracket"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.sViewColumn" for="field_sViewColumn">S View Column</label>
            <input type="text" class="form-control" name="sViewColumn" id="field_sViewColumn"
                [(ngModel)]="acctRuleConditions.sViewColumn"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.operator" for="field_operator">Operator</label>
            <input type="text" class="form-control" name="operator" id="field_operator"
                [(ngModel)]="acctRuleConditions.operator"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.value" for="field_value">Value</label>
            <input type="text" class="form-control" name="value" id="field_value"
                [(ngModel)]="acctRuleConditions.value"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.closeBracket" for="field_closeBracket">Close Bracket</label>
            <input type="text" class="form-control" name="closeBracket" id="field_closeBracket"
                [(ngModel)]="acctRuleConditions.closeBracket"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.logicalOperator" for="field_logicalOperator">Logical Operator</label>
            <input type="text" class="form-control" name="logicalOperator" id="field_logicalOperator"
                [(ngModel)]="acctRuleConditions.logicalOperator"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.createdBy" for="field_createdBy">Created By</label>
            <input type="number" class="form-control" name="createdBy" id="field_createdBy"
                [(ngModel)]="acctRuleConditions.createdBy"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.createdDate" for="field_createdDate">Created Date</label>
            <div class="d-flex">
                <input id="field_createdDate" type="datetime-local" class="form-control" name="createdDate" [(ngModel)]="acctRuleConditions.createdDate"
                  />
            </div>
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.lastUpdatedBy" for="field_lastUpdatedBy">Last Updated By</label>
            <input type="number" class="form-control" name="lastUpdatedBy" id="field_lastUpdatedBy"
                [(ngModel)]="acctRuleConditions.lastUpdatedBy"  />
        </div>
        <div class="form-group">
            <label class="form-control-label" jhiTranslate="agreeGatewayV1App.acctRuleConditions.lastUpdatedDate" for="field_lastUpdatedDate">Last Updated Date</label>
            <div class="d-flex">
                <input id="field_lastUpdatedDate" type="datetime-local" class="form-control" name="lastUpdatedDate" [(ngModel)]="acctRuleConditions.lastUpdatedDate"
                  />
            </div>
        </div>

    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" (click)="clear()">
            <span class="fa fa-ban"></span>&nbsp;<span jhiTranslate="entity.action.cancel">Cancel</span>
        </button>
        <button type="submit" [disabled]="editForm.form.invalid || isSaving" class="btn btn-primary">
            <span class="fa fa-save"></span>&nbsp;<span jhiTranslate="entity.action.save">Save</span>
        </button>
    </div>
</form>
