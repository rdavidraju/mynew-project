<md-card class="express-mode-steps ">
    <div class="row botom-dotted-blue-border component-title">
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8 pageTitle pageListTitle">
            <h2>Express Mode</h2>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
            <button md-icon-button md-button class="pull-right color-gray" mdTooltip="Cancel">
                <md-icon>clear</md-icon>
            </button>
        </div>
    </div>
    <div class="row process-title-row">
        <div class="process-Title col-md-3">
            <md-input-container>
                <input mdInput (blur)="checkProcessName()" [(ngModel)]="currentProcess.name"
                placeholder="Process Name" name="processName" required>
            </md-input-container>
        </div>
    </div>
    <div style="padding: 0px 10px 10px 10px;">
        <p-steps [model]="items" [(activeIndex)]="activeIndex" [readonly]="true"></p-steps>
    </div>
</md-card>
<div class="express-mode-content split-example">
    <div style="width:100%; height:100%">
		<router-outlet name="content"></router-outlet>
	</div>
</div>