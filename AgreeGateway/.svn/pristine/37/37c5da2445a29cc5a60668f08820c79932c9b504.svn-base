<div>
    <h2>
        <span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.home.title">Approval Group Members</span>
        <button class="btn btn-primary float-right jh-create-entity create-approval-group-members" [routerLink]="['/', { outlets: { popup: ['approval-group-members-new'] } }]">
            <span class="fa fa-plus"></span>
            <span class="hidden-sm-down"  jhiTranslate="agreeGatewayV1App.approvalGroupMembers.home.createLabel">
            Create new Approval Group Members
            </span>
        </button>
    </h2>
    <jhi-alert></jhi-alert>
    <div class="row">
        <div class="col-sm-12">
            <form name="searchForm" class="form-inline">
                <div class="input-group w-100 mt-3">
                    <input type="text" class="form-control" [(ngModel)]="currentSearch" id="currentSearch" name="currentSearch" placeholder="{{ 'agreeGatewayV1App.approvalGroupMembers.home.search' | translate }}">
                    <button class="input-group-addon btn btn-info" (click)="search(currentSearch)">
                        <span class="fa fa-search"></span>
                    </button>
                    <button class="input-group-addon btn btn-danger" (click)="clear()" *ngIf="currentSearch">
                        <span class="fa fa-trash-o"></span>
                    </button>
                </div>
            </form>
        </div>
    </div>
    <br/>
    <div class="table-responsive" *ngIf="approvalGroupMembers">
        <table class="table table-striped">
            <thead>
            <tr jhiSort [(predicate)]="predicate" [(ascending)]="reverse" [callback]="transition.bind(this)">
            <th jhiSortBy="id"><span jhiTranslate="global.field.id">ID</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="groupId"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.groupId">Group Id</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="seq"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.seq">Seq</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="userId"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.userId">User Id</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="startDate"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.startDate">Start Date</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="endDate"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.endDate">End Date</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="createdBy"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.createdBy">Created By</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="createdDate"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.createdDate">Created Date</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="lastUpdatedBy"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.lastUpdatedBy">Last Updated By</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="lastUpdatedDate"><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.lastUpdatedDate">Last Updated Date</span> <span class="fa fa-sort"></span></th>
            <th></th>
            </tr>
            </thead>
            <tbody>
            <tr *ngFor="let approvalGroupMembers of approvalGroupMembers ;trackBy: trackId">
                <td><a [routerLink]="['../approval-group-members', approvalGroupMembers.id ]">{{approvalGroupMembers.id}}</a></td>
                <td>{{approvalGroupMembers.groupId}}</td>
                <td>{{approvalGroupMembers.seq}}</td>
                <td>{{approvalGroupMembers.userId}}</td>
                <td>{{approvalGroupMembers.startDate | date:'mediumDate'}}</td>
                <td>{{approvalGroupMembers.endDate | date:'mediumDate'}}</td>
                <td>{{approvalGroupMembers.createdBy}}</td>
                <td>{{approvalGroupMembers.createdDate | date:'medium'}}</td>
                <td>{{approvalGroupMembers.lastUpdatedBy}}</td>
                <td>{{approvalGroupMembers.lastUpdatedDate | date:'medium'}}</td>
                <td class="text-right">
                    <div class="btn-group flex-btn-group-container">
                        <button type="submit"
                                [routerLink]="['../approval-group-members', approvalGroupMembers.id ]"
                                class="btn btn-info btn-sm">
                            <span class="fa fa-eye"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.view">View</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'approval-group-members/'+ approvalGroupMembers.id + '/edit'} }]"
                                replaceUrl="true"
                                class="btn btn-primary btn-sm">
                            <span class="fa fa-pencil"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.edit">Edit</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'approval-group-members/'+ approvalGroupMembers.id + '/delete'} }]"
                                replaceUrl="true"
                                class="btn btn-danger btn-sm">
                            <span class="fa fa-remove"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.delete">Delete</span>
                        </button>
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
        <!-- Pager is not implemented yet, so this is normal pagination instead -->
        <div *ngIf="approvalGroupMembers && approvalGroupMembers.length">
            <div class="row justify-content-center">
                <jhi-item-count [page]="page" [total]="queryCount" [itemsPerPage]="itemsPerPage"></jhi-item-count>
            </div>
            <div class="row justify-content-center">
                <ngb-pagination [collectionSize]="totalItems" [(page)]="page"  (pageChange)="loadPage(page)"></ngb-pagination>
            </div>
        </div>
</div>
