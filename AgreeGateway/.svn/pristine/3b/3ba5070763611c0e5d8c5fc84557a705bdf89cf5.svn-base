                </md-card-header>
                <div style="display:flex;">
                    <div class="col-md-8" style="padding: 7px;">
                        <div style="display: block;height:250px !important;padding-top: 14px;">
                            <canvas baseChart [datasets]="lineChartDataJournals" [labels]="lineChartLabelsJE" [options]="lineChartOptions" [colors]="lineChartColors"
                                [legend]="lineChartLegendJE" [chartType]="lineChartType" (chartHover)="chartHovered($event)"
