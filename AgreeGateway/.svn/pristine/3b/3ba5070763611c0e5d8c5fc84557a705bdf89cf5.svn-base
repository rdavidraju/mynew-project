<div class="component-body dashboardV5">
    <md-card id="dashboardNavSticky">
        <!-- id="dashboardNavSticky" -->
        <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-left: 0px;padding: 10px;">
            <div class="col-lg-3 col-md-3 processSelectionTitle">
                <!-- <div>Selected Process : {{selectedProcessName}}</div> -->
                <md-select placeholder="" [(ngModel)]="selectedProcess" name="selectedProcess" style="padding-top: 0px;width: 200px;">
                    <!--   padding-left: 8px;  -->
                    <md-option *ngFor="let col of processesList" [value]="col.id">
                        {{ col.processName }}
                    </md-option>
                </md-select>
            </div>
            <!-- <div class="col-lg-8 col-md-8 mainNav" style="margin-top: -10px;margin-bottom: -14px;">
                <md-tab-group style="float:right;" (focusChange)="changeModuleType($event);" [selectedIndex]="selectedModuleType">
                    <md-tab label="Reconciliation"></md-tab>
                    <md-tab label="Accounting"></md-tab>
                </md-tab-group>
            </div> -->
            <div class="col-lg-1 col-md-1" style="position: absolute;right: 0;top: 5px;">
                <button md-icon-button class="pull-right" aria-hidden="true" mdTooltip="Filter Data" (click)="openSideBar('Filter Data');">
				    <md-icon>filter_list</md-icon>
				</button>
            </div>
        </div>
    </md-card>
    <div class="row" style="margin-left: -5px;margin-right: -5px;margin-top: 7px;">
        <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
            <md-card id="dashboardNavSticky1">
                <!-- <div class=" col-lg-1 col-md-1"></div> -->
                <div class="sideProgess extractionStatusCls" (click)="displaySelectedProcess('extraction');">
                    <circle-progress [percent]="90" [radius]="40" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#ffa1b5'"
                        [innerStrokeColor]="'#ededed'" [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Extraction'"
                        [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="10"></circle-progress>
                </div>
                <div class="sideProgess transformationStatusCls" (click)="displaySelectedProcess('transformation');">
                    <circle-progress [percent]="85" [radius]="40" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#86c7f3'"
                        [innerStrokeColor]="'#ededed'" [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Transformation'"
                        [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="10"></circle-progress>
                </div>
                <div class="sideProgess reconciliationStatusCls" (click)="displaySelectedProcess('reconciliation');">
                    <circle-progress [percent]="80" [radius]="40" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'rgba(92, 184, 92, 0.85)'"
                        [innerStrokeColor]="'#ededed'" [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Reconciliation'"
                        [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="10"></circle-progress>
                </div>
                <div class="sideProgess accountingStatusCls" (click)="displaySelectedProcess('accounting');">
                    <circle-progress [percent]="71" [radius]="40" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#93d9d9'"
                        [innerStrokeColor]="'#ededed'" [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Accounting'"
                        [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="10"></circle-progress>
                </div>
                <div class="sideProgess journalsStatusCls" (click)="displaySelectedProcess('journals');">
                    <circle-progress [percent]="50" [radius]="40" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#ff9f40'"
                        [innerStrokeColor]="'#ededed'" [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Journals'"
                        [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="10"></circle-progress>
                </div>
                <!-- <div class=" col-lg-1 col-md-1"></div> -->
            </md-card>
        </div>

        <div class="col-lg-10 col-md-10 col-sm-9 col-xs-12">
            <md-card id="extractionDiv">
                <md-card-header>
                    <md-card-title>Extraction</md-card-title>
                </md-card-header>
                <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-left: 0px;margin-top: 10px;padding: 0px;">
                    Extraction Data
                </div>
            </md-card>
            <md-card id="transformationDiv" style="margin-top: 7px;">
                <md-card-header>
                    <md-card-title>Transformation</md-card-title>
                </md-card-header>
                <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-left: 0px;margin-top: 10px;padding: 0px;">
                    Transformation Data
                </div>
            </md-card>
            <md-card id="reconciliationAct" style="margin-top: 7px;">
                <md-card-header>
                    <md-card-title style="display: -webkit-box;">
