                                    [showSubtitle]="true" [subtitle]="'Extraction'" [titleFontSize]="20" [unitsFontSize]="15"
                                    [subtitleFontSize]="12"></circle-progress>
                                <!-- <md-progress-spinner class="example-margin" style="margin-left: 43px;" [color]="extractionColor" [mode]="mode" [value]="extractionValue"
										 [strokeWidth]=3>
										</md-progress-spinner>
										<div style="text-align: center;">Extraction - {{extractionValue}}%</div> -->
                            </div>
                            <div class=" col-lg-4 col-md-4 transformationCol text-center">
                                <circle-progress [percent]="transformationValue" [radius]="50" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6"
                                    [outerStrokeColor]="'#86c7f3'" [innerStrokeColor]="'rgba(134, 199, 243, 0.53)'" [animation]="true"
                                    [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Transformation'" [titleFontSize]="20"
                                    [unitsFontSize]="15" [subtitleFontSize]="12"></circle-progress>
                                <!-- <md-progress-spinner class="example-margin" [color]="transformationColor" [mode]="mode" [value]="transformationValue" [strokeWidth]=3>
										</md-progress-spinner>
										<div>Transformation - {{transformationValue}}%</div> -->
                            </div>
                            <div class=" col-lg-4 col-md-4 reconCol text-center">
                                <circle-progress [percent]="reconciliationValue" [radius]="50" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6"
                                    [outerStrokeColor]="'rgba(92, 184, 92, 0.85)'" [innerStrokeColor]="'rgba(92, 184, 92, 0.34)'"
                                    [animation]="true" [animationDuration]="300" [showSubtitle]="true" [subtitle]="'Reconciliation'"
                                    [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="12"></circle-progress>
                                <!-- <md-progress-spinner class="example-margin" [color]="reconColor" [mode]="mode" [value]="reconciliationValue" [strokeWidth]=3>
										</md-progress-spinner>
										<div>Reconciliation - {{reconciliationValue}}%</div> -->
                            </div>
                            <div class=" col-lg-2 col-md-2">
                            </div>
                            <div class=" col-lg-4 col-md-4 accountingCol text-center">
                                <circle-progress [percent]="accountingValue" [radius]="50" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#93d9d9'"
                                    [innerStrokeColor]="'rgba(147, 217, 217, 0.54)'" [animation]="true" [animationDuration]="300"
                                    [showSubtitle]="true" [subtitle]="'Accounting'" [titleFontSize]="20" [unitsFontSize]="15"
                                    [subtitleFontSize]="12"></circle-progress>
                                <!-- <md-progress-spinner class="example-margin" [color]="accountingColor" [mode]="mode" [value]="accountingValue" [strokeWidth]=3>
										</md-progress-spinner>
										<div>Accounting - {{accountingValue}}%</div> -->
                            </div>
                            <div class=" col-lg-4 col-md-4 journalCol text-center">
                                <circle-progress [percent]="journalsValue" [radius]="50" [space]="-6" [outerStrokeWidth]="6" [innerStrokeWidth]="6" [outerStrokeColor]="'#ff9f40'"
                                    [innerStrokeColor]="'rgba(255, 159, 64, 0.47)'" [animation]="true" [animationDuration]="300"
                                    [showSubtitle]="true" [subtitle]="'Journals'" [titleFontSize]="20" [unitsFontSize]="15" [subtitleFontSize]="12"></circle-progress>
                                <!-- <md-progress-spinner class="example-margin" [color]="journalsColor" [mode]="mode" [value]="journalsValue" [strokeWidth]=3>
										</md-progress-spinner>
										<div>Journals - {{journalsValue}}%</div> -->
                            </div>
                        </div>
                    </div>
                </md-card-content>
            </md-card>
        </div>
    <!-- </md-card> -->


    <!-- SIDE BAR -->
    <p-sidebar [(visible)]="visibleSidebar2" position="right" [baseZIndex]="10000">
        <div *ngIf="openModuleSetting == 'Filter Data'">
            <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                <div class="col-md-4" style="margin-top: 12px;">Processes :</div>
                <div class="col-md-8">
                    <!-- <md-select placeholder="" [(ngModel)]="selectedProcess" name="selectedProcess" style="padding-top: 0px; padding-left: 8px;">
                        <md-option *ngFor="let col of processesList" [value]="col.id">
                            {{ col.processName }}
                        </md-option>
                    </md-select> -->
                    <!-- <md-list *ngFor="let col of processesList">
                        <md-list-item>{{ col.processName }}</md-list-item>
                    </md-list> -->
                    <perfect-scrollbar style="height: 100px;">
                        <md-nav-list>
                            <a md-list-item *ngFor="let col of processesList" (click)="selectedProcessFun(col);" [ngClass]="{'nav-active': col == activeItem}"> {{ col.processName }} </a>
                        </md-nav-list>
                    </perfect-scrollbar>
                </div>
            </div>
            <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                <div class="col-md-12" style="margin-top: auto; margin-bottom: auto;">
                    <md-checkbox [(ngModel)]="processFilter.asOfValue">As Of Today</md-checkbox>
                </div>
                <div class="col-md-12" *ngIf="processFilter.asOfValue == false" style="display: flex;padding-left: 33px;">
                    <div class="datePickerWidth">
                        <md-form-field> <input mdInput [mdDatepicker]="stpicker" placeholder="From date" [ngClass]="{'color-black' : isViewOnly}"
                                [disabled]="isViewOnly" [(ngModel)]="processFilter.startDate" name="startDate" required>
                            <md-datepicker-toggle mdSuffix [for]="stpicker" *ngIf="!isViewOnly"></md-datepicker-toggle>
                            <md-datepicker #stpicker></md-datepicker>
                        </md-form-field>
                    </div>
                    <div class="datePickerWidth">
                        <md-form-field> <input mdInput [mdDatepicker]="edpicker" placeholder="To date" [min]="processFilter.startDate" [ngClass]="{'color-black' : isViewOnly}"
                                [disabled]="isViewOnly" [(ngModel)]="processFilter.endDate" name="endDate">
                            <md-datepicker-toggle mdSuffix [for]="edpicker" *ngIf="!isViewOnly"></md-datepicker-toggle>
                            <md-datepicker #edpicker></md-datepicker>
                        </md-form-field>
                    </div>
                </div>
            </div>
