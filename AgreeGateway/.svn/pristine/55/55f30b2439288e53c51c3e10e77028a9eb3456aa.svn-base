@font-face {
  font-family: 'recon';
  src:  url('fonts/recon.eot?avhk6d');
  src:  url('fonts/recon.eot?avhk6d#iefix') format('embedded-opentype'),
    url('fonts/recon.ttf?avhk6d') format('truetype'),
    url('fonts/recon.woff?avhk6d') format('woff'),
    url('fonts/recon.svg?avhk6d#recon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="rec-font-"], [class*=" rec-font-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'recon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

