            <button md-button class="color-gray" style="float: right;color: green !important;" *ngIf="selectedBatchDetails.status == 'InProcess'"
                mdTooltip="Approve" (click)="approveSelectedBatch(selectedBatchDetails)"><md-icon>thumb_up</md-icon>  Approve</button>
            <button md-button class="color-gray" style="float: right" (click)="viewDetailedReport(selectedBatchDetails.id);selectedValues = ''"
                mdTooltip="View Detailed Report"><md-icon>description</md-icon>  View Report</button>
        </div>
    </p-dialog>
</div>

