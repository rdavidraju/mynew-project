                                <div *ngIf="i < 4" style="display: flex;">
                                    <div class="col-md-8">
                                        <!-- [ngClass]="{'classCurrentApp':approval.status == 'Rejected'}" -->
                                        <div *ngIf="approval.approvalStatus == 'Approved'" style="margin-left: auto; margin-right: auto; text-align: center;">
                                            <img src='userImages/app.svg' style="height: 57px;" mdTooltip="{{approval.approvalStatus}}">
                                        </div>
                                        <div *ngIf="approval.approvalStatus == 'Rejected'" style="margin-left: auto; margin-right: auto; text-align: center;">
                                            <img src='userImages/rej.svg' style="height: 57px;" mdTooltip="{{approval.approvalStatus}}">
                                        </div>
                                        <div *ngIf="approval.approvalStatus == ''" style="margin-left: auto; margin-right: auto; text-align: center;">
                                            <img src='userImages/user.svg' style="height: 57px;" mdTooltip="{{approval.approvalStatus}}">
                                        </div>
                                        <div *ngIf="approval.approvalStatus == 'InProcess'" style="margin-left: auto; margin-right: auto; text-align: center;">
                                            <img src='userImages/currentApp.svg' style="height: 57px;" mdTooltip="{{approval.approvalStatus}}">
                                        </div>
                                        <div style="text-align: center;white-space: nowrap;" [ngClass]="{'approversClass':approval.approvalStatus == 'Approved','inprocessApprover':approval.approvalStatus == '','text-danger':approval.approvalStatus == 'Rejected'}">{{approval.name}}</div>
                                        <div style="text-align: center;white-space: nowrap;" [ngClass]="{'approversClass':approval.approvalStatus == 'Approved','inprocessApprover':approval.approvalStatus == '','text-danger':approval.approvalStatus == 'Rejected'}"
                                            mdTooltip="{{approval.approvalDate | date:'medium'}}">{{approval.approvalDate | date:'mediumDate'}}</div>
                                    </div>
                                    <div class="col-md-4" style="margin-left: auto;margin-right: auto;text-align: center;">
                                        <i *ngIf='approval.next == true' class="material-icons" style="position: relative;top: 13px;">trending_flat</i>
                                    </div>
                                </div>
                            </div>
                        </div>
