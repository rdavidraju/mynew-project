<div class="container-fluid component-body file-templates">
    <div class="row botom-dotted-blue-border component-title">
        <div class="col-md-6 pageTitle" style="padding-left: 0px;display:flex;">
            <!-- <div class="col-md-8"> -->
                <h2>Approval's Work Queue</h2>
            <!-- </div> -->
            <!-- <div class="col-md-4">
                <md-select placeholder="Select Module" [(ngModel)]="selectedModuleType" name="moduleType" style="width: 80%;margin-top: -7px;"
                    [ngClass]="{'color-black' : isViewOnly}" [disabled]="isViewOnly" required>
                    <md-option *ngFor="let col of moduleType" [value]="col.value" (click)="fetchNotificationBatchList('IN_PROCESS');">
                        {{ col.name }}
                    </md-option>
                </md-select>
            </div> -->
        </div>
        <div class="col-md-6 ftlSearch">
            <button md-icon-button class="pull-right search-icon-body" mdTooltip="Search">
				<md-icon>search</md-icon>
			</button>
            <md-input-container class="pull-right hidethis">
                <input class="mySearchBox" mdInput #gb placeholder="What're we looking for?">
            </md-input-container>
        </div>
    </div>
    <div class="container-fluid component-body">
        <!-- <div class="split-pane-content-secondary">  -->
        <vertical-split-pane primary-component-minsize="35" primary-component-initialratio="0.2">
            <div class="split-pane-content-primary sideBarNotificationQue">
                <div class="botom-dotted-blue-border pageTitle row" style="margin-left: 0px;margin-right: 4px;margin-top: 18px;">
                    <h5 style="margin-left: -5px;">Hierarcial View</h5>
                </div>
                <div class="hierarchyClass">
                    <p-tree [value]="sideBarHierarchyList" selectionMode="single" [(selection)]="selectedFile" (onNodeSelect)="nodeSelect($event)"
                        (onNodeUnselect)="nodeUnselect($event)"></p-tree>
                </div>
            </div>
            <div class="split-pane-content-secondary">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="z-index: 1;top: 14px;display:flex;">
                    <div class="col-md-4">
                        <md-select placeholder="Select Module" [(ngModel)]="selectedModuleType" name="moduleType" style="width: 50%;margin-top: -7px;"
                            [ngClass]="{'color-black' : isViewOnly}" [disabled]="isViewOnly" required>
                            <md-option *ngFor="let col of moduleType" [value]="col.value" (click)="fetchNotificationBatchList('IN_PROCESS');">
                                {{ col.name }}
                            </md-option>
                        </md-select>
                    </div>
                    <div class="col-md-8">
                        <popover-content #myPopover title="Reassign" placement="left" [animation]="true" [closeOnClickOutside]="false" [popoverCloseOnMouseOutside]="false">
                            <div class="col-md-12" style="display: flex;">
                                <div class="col-md-6" style="white-space:nowrap;margin-top: auto;margin-bottom: auto;top: 10px;">Select User :</div>
                                <div class="col-md-6">
                                    <md-select placeholder="" name="userslists" [ngClass]="{'color-black' : isViewOnly}" [(ngModel)]="selectedUser">
                                        <md-option *ngFor="let col of usersList;" [value]="col.id">{{col.firstName}} {{col.lastName}}</md-option>
                                    </md-select>
                                </div>
                            </div>
                            <div style="padding: 13px 2px 5px 5px;">
                                <button md-button style="float: right;" (click)="myPopover.hide();" (click)="reassignSelectedBatch();">Ok</button>
                                <button md-button style="float: right;" (click)="myPopover.hide();">Cancel</button>
                            </div>
                        </popover-content>
                        <button md-button class="rejectApprover rejectedList" mdTooltip="View Rejected Notification" style="color: red;float: right;"
                            (click)="fetchNotificationBatchList('REJECTED');displayCheckBox = false;" *ngIf="selectedBatchRows.length<1">Rejected</button>
                        <button md-button class="approversClass approvedList" mdTooltip="View Approved Notification" style="color: green;float: right;"
                            (click)="fetchNotificationBatchList('APPROVED');displayCheckBox = false;" *ngIf="selectedBatchRows.length<1">Approved</button>
                        <button md-button class="inprocessApprover inProcessList" mdTooltip="View In Process Notification"
                            style="color: #1e88e8;float: right;" (click)="fetchNotificationBatchList('IN_PROCESS');displayCheckBox = true;"
                            *ngIf="selectedBatchRows.length<1">In Process</button>
                        <button md-button class="classAllList" style="float: right;" mdTooltip="View All Notification" (click)="fetchNotificationBatchList();displayCheckBox = false;"
                            *ngIf="selectedBatchRows.length<1">All</button>
                        
                        
                        
                        <!-- <button md-icon-button class="ripple color-gray pull-right" mdTooltip="Advanced Filter" aria-hidden="true">
								<i class="fa fa-filter fa-2x" aria-hidden="true"></i>
                            </button> -->
                        <button class="color-gray" style="float: right;" *ngIf="selectedBatchRows.length>0" md-button [popover]="myPopover"><md-icon>redo</md-icon>  Reassign</button>
                        <button class="color-gray" style="float: right;" *ngIf="selectedBatchRows.length>0" md-button (click)="rejectSelectedBatch();"><md-icon>thumb_down</md-icon>  Reject</button>
                        <button class="color-gray"  *ngIf="selectedBatchRows.length>0" md-button (click)="approveSelectedBatch();" style="color: green !important;float: right;"><md-icon>thumb_up</md-icon>  Approve</button>
                        
                        
                        <!-- <button class="color-gray" *ngIf="selectedBatchRows.length>0" md-button><md-icon>format_list_bulleted</md-icon>  More</button> -->
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"><!--  style="margin-top: 41px;" -->
                    <div style="text-align:right" class="show-hide-class">
                        <p-multiSelect [options]="columnOptions" [(ngModel)]="notificationBatchTableColumns"></p-multiSelect>
                    </div>
                    <p-dataTable [value]="notificationBatches" reorderableColumns="true" resizableColumns="true" columnResizeMode="expand" scrollable="true"
                        scrollHeight={{TemplatesHeight}} [responsive]="true" [headerCheckboxToggleAllPages]="true" [globalFilter]="gb"
                        #dt [rows]="15" [paginator]="false" [pageLinks]="3" [rowsPerPageOptions]="[5,10,20,50]" [(selection)]="selectedBatchRows">
                        <!-- <p-column  [style]="{'width':'38'}" [sortable]="true">
                     <ng-template let-col let-row="rowData" pTemplate="body" selectionMode="multiple">
                        <p-checkbox ></p-checkbox>
                     </ng-template>
                </p-column> -->
                        <p-column [style]="{'width':'38px'}" selectionMode="multiple" *ngIf="displayCheckBox"></p-column>
                        <p-column field="notificationName" header="Notification Name" [style]="{'width':'280px'}" [sortable]="true">
                            <ng-template let-col let-row="rowData" pTemplate="body">
                                <a (click)="displaySelectedBatchDetails(row.id);">{{row[col.field]}}</a>
                            </ng-template>
                        </p-column>
                        <p-column field="notificationDate" header="Notification Date" [style]="{'width':'150px'}" [sortable]="true">
                            <ng-template let-col let-row="rowData" pTemplate="body">{{row[col.field] | date:'medium'}}
                            </ng-template>
                        </p-column>
                        <p-column field="module" header="Module Type" [style]="{'width':'150px'}" [sortable]="true" *ngIf="selectedModuleType == 'ALL'">
                            <ng-template let-col let-row="rowData" pTemplate="body">{{row[col.field]}}
                            </ng-template>
                        </p-column>
                        <p-column *ngFor="let col of notificationBatchTableColumns" [style]="{'width':col.width,'text-align':col.align}" [field]="col.field"
                            [header]="col.header" [sortable]="true">
                        </p-column>
                        <p-column [style]="{'width':'110px' }" field="status" header="Status" styleClass="text-center" styleClass="center" [sortable]="true">
                            <ng-template let-col let-row="rowData" pTemplate="body">
                                <div class="badge badge-success " *ngIf="row.statusCode == 'APPROVED'">{{row[col.field]}}</div>
                                <div class="badge badge-danger " *ngIf="row.statusCode == 'REJECTED'">{{row[col.field]}}</div>
                                <div class="badge  " *ngIf="row.statusCode == 'IN_PROCESS'" style="background-color: #1e88e8;">{{row[col.field]}}</div>
                            </ng-template>
                        </p-column>
                    </p-dataTable>
                    <md-paginator *ngIf="batchListRecordsLength>0" [length]="batchListRecordsLength" [pageSize]="itemsPerPage" [pageSizeOptions]="pageSizeOptions"
                        [pageIndex]="page" (page)="onPaginateChange($event)">
                    </md-paginator>
                </div>
            </div>
        </vertical-split-pane>
        <!-- </div> -->
    </div>
</div>
<!-- HTML for dialog box -->

<div class="target-data">
    <p-dialog header="Transaction Details" [(visible)]="display" [resizable]="true" [responsive]="true">
        <div class="row" style="margin-left: -4px;">
            <div class="col-md-3 col-xs-6 form-group">
