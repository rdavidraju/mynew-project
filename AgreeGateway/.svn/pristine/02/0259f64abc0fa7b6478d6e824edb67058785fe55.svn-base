@import '~@angular/material/theming';


@include mat-core();


@import 'themes/blue-deeporange.scss';
@import 'themes/light.scss';
@import 'themes/default-dark-light.scss';
@import 'themes/dark-night.scss';
@import 'themes/full-light.scss';
@import 'themes/simple-light.scss';

.light-theme {
  @include angular-material-theme($light-theme);
  @include light-theme($light-theme);
}

.dark-light-theme {
  @include angular-material-theme($dark-light-theme);
  @include dark-light-theme($dark-light-theme);
}

.deep-orange-theme {
  @include angular-material-theme($blue-orange-theme);
  @include blue-orange-theme($blue-orange-theme);
}

.dark-night-theme {
  @include angular-material-theme($dark-night-theme);
  @include dark-night-theme($dark-night-theme);
}

.full-light-theme {
  @include angular-material-theme($full-light-theme);
  @include full-light-theme($full-light-theme);
}

.simple-light {
  @include angular-material-theme($simple-light);
  @include simple-light($simple-light);
}


