            <li *ngSwitchCase="true" ngbDropdown class="nav-item dropdown pointer">
                <a class="nav-link dropdown-toggle" ngbDropdownToggle href="javascript:void(0);" id="message-menu" (mouseenter)="loadschedularList()">
                  <span>
                    <i class="fa fa-comment-o jobsList" aria-hidden="true"></i>
                    <span class="disblock">Jobs Schedule</span>
                    <b class="caret"></b>
                  </span>
                </a>
                <div class="dropdown-menu jobs-window" ngbDropdownMenu>
                  <!--   <div class="notifications-head" style="width:100%; height:10%">
                        <md-slide-toggle class="toggle-button">Show Upcoming Schedules
                        </md-slide-toggle>
                    </div> -->
                    <table class="jobs-list table table-hover">
                        <thead>
                            <tr>
                                <th>Job Name</th>
                                <th>Program Name</th>
                                <th>Frequency</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr *ngFor="let item of schedularList;" [mdTooltip]="'Parameters : '+item.parametes + ' Schedule Type : '+item.schedulerName">
                                <td>{{item.jobName}}</td>
                                <td>{{item.programName}}</td>
                                <td>{{item.frequencyType}}</td>
                                <td>{{item.oozieStatus}}</td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="gotoAllJobsList">
                        <button md-button [routerLink]="['/jobs', {outlets: {'content': ['schedulars-list']}}]" class="pull-right">Show All</button>
                    </div>

                </div>
            </li>
            <li *ngSwitchCase="true" ngbDropdown class="nav-item dropdown pointer">

                <a class="nav-link dropdown-toggle" ngbDropdownToggle href="javascript:void(0);" id="notification-menu">
                  <span>
                    <i class="fa fa-bell-o" aria-hidden="true"></i>
                    <span class="disblock">Notifications</span>
                    <b class="caret"></b>
                  </span>
                </a>
                <ul class="dropdown-menu dropdown-menu-right" ngbDropdownMenu>

                </ul>
            </li>
            <li ngbDropdown class="nav-item dropdown pointer">
                <a class="nav-link dropdown-toggle" ngbDropdownToggle href="javascript:void(0);" id="account-menu"
                	(mouseenter)="getuserinfo()">
                  <span *ngIf="!getImageUrl()">
                    <i class="fa fa-user" aria-hidden="true"></i>
                    <span class="disblock">Account</span>
                    <b class="caret"></b>
                  </span>
                  <span *ngIf="getImageUrl()">
                      <img [src]="getImageUrl()" class="profile-image img-circle" alt="Avatar">
                  </span>
                </a>
                <ul class="dropdown-menu dropdown-menu-right" [ngClass]="isAuthenticated() ? 'userList' : 'noClass'" ngbDropdownMenu>
                    <ul class="splitListUser">
                        <li *ngSwitchCase="true">
                            <a class="dropdown-item">
                                <span>{{UserData?.lastName}} {{UserData?.firstName}}</span>
                                <span style="display:block;font-size:13px">{{UserData?.email}}</span>
                            </a>
                        </li>
                        <li *ngSwitchCase="true" style="width:30%;">
                            <a class="dropdown-item curPointer" style="height:45px" routerLink="settings" routerLinkActive="active" (click)="collapseNavbar()">
                                <i class="fa fa-fw fa-wrench" aria-hidden="true" style="margin: 10px 0px 0px 0px;"></i>
                            </a>
                        </li>
                    </ul>
                    <ul class="splitList">
                        <li *ngSwitchCase="true">
                            <a routerLink="password" routerLinkActive="active" (click)="collapseNavbar()" style="margin-left:10px;">
                                <i class="fa fa-fw fa-clock-o" aria-hidden="true"></i>
                                <span>Password</span>
                            </a>
                        </li>
                        <li *ngSwitchCase="true">
                            <a (click)="logout()" id="logout" style="margin-left:10px;">
                                <i class="fa fa-fw fa-sign-out" aria-hidden="true"></i>
                                <span>Sign out</span>
                            </a>
                        </li>
                    </ul>
                    <li *ngSwitchCase="false">
                        <a class="dropdown-item" (click)="login()" id="login">
                            <i class="fa fa-fw fa-sign-in" aria-hidden="true"></i>
                            <span>Sign in</span>
                        </a>
                    </li>
                    <li *ngSwitchCase="false">
                        <a class="dropdown-item" routerLink="register" routerLinkActive="active" (click)="collapseNavbar()">
                            <i class="fa fa-fw fa-registered" aria-hidden="true"></i>
                            <span>Register</span>
                        </a>
                    </li>
                </ul>
            </li>
        </ul>
