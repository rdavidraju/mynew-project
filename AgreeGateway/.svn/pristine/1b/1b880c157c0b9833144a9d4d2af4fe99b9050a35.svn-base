<style>
    .userList {
        width: 250px !important;
        height: 150px !important;
        padding: 0;
    }

    .splitList li:hover {
        background: #2197ad !important;
    }

    .splitList li:hover a {
        color: white !important;
    }

    .userList .dropdown-item:hover {
        background: white;
        cursor: default;
    }

    .dropdown-item.active{
        background: #2197ad !important;
    }

    .curPointer:hover {
        cursor: pointer !important;
    }

    .splitListUser>li {
        float: left;
        width: 70%;
        padding: 4px;
    }

    .splitList {
        clear: both;
        bottom: 0px;
        position: absolute;
    }

    .splitList li {
        float: left;
        border: 1px solid lightgray;
        padding: 6px;
        padding-right: 22px;
        border-bottom: 0;
        border-left: 0;
    }

    .splitList li a {
        text-decoration: none;
        color: #292b2c;
    }
</style>
