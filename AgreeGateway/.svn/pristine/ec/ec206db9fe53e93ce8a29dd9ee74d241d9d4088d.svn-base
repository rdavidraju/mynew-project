package com.nspl.app.web.rest.dto;

import java.time.LocalDate;
import java.util.List;

public class FunctionalityAndRolesDTO {
	
	private Long id;
    private String funcName;
    private String funcDesc;
    private String funcType;
    private LocalDate funcStartDate;
    private LocalDate funcEndDate;
    private Long tenantId;
    private boolean FuncActiveFlag;
    private List<RoleAssignmentDTO> funcRoles;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getFuncName() {
		return funcName;
	}
	public void setFuncName(String funcName) {
		this.funcName = funcName;
	}
	public String getFuncDesc() {
		return funcDesc;
	}
	public void setFuncDesc(String funcDesc) {
		this.funcDesc = funcDesc;
	}
	public String getFuncType() {
		return funcType;
	}
	public void setFuncType(String funcType) {
		this.funcType = funcType;
	}
	public LocalDate getFuncStartDate() {
		return funcStartDate;
	}
	public void setFuncStartDate(LocalDate funcStartDate) {
		this.funcStartDate = funcStartDate;
	}
	public LocalDate getFuncEndDate() {
		return funcEndDate;
	}
	public void setFuncEndDate(LocalDate funcEndDate) {
		this.funcEndDate = funcEndDate;
	}
	public Long getTenantId() {
		return tenantId;
	}
	public void setTenantId(Long tenantId) {
		this.tenantId = tenantId;
	}
	public boolean isFuncActiveFlag() {
		return FuncActiveFlag;
	}
	public void setFuncActiveFlag(boolean funcActiveFlag) {
		FuncActiveFlag = funcActiveFlag;
	}
	public List<RoleAssignmentDTO> getFuncRoles() {
		return funcRoles;
	}
	public void setFuncRoles(List<RoleAssignmentDTO> funcRoles) {
		this.funcRoles = funcRoles;
	}
  

}
