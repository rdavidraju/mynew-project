<div>
    <h2>
        <span jhiTranslate="agreeGatewayV1App.templAttributeMapping.home.title">Templ Attribute Mappings</span>
        <button class="btn btn-primary float-right jh-create-entity create-templ-attribute-mapping" [routerLink]="['/', { outlets: { popup: ['templ-attribute-mapping-new'] } }]">
            <span class="fa fa-plus"></span>
            <span class="hidden-sm-down"  jhiTranslate="agreeGatewayV1App.templAttributeMapping.home.createLabel">
            Create new Templ Attribute Mapping
            </span>
        </button>
    </h2>
    <jhi-alert></jhi-alert>
    <div class="row">
        <div class="col-sm-12">
            <form name="searchForm" class="form-inline">
                <div class="input-group w-100 mt-3">
                    <input type="text" class="form-control" [(ngModel)]="currentSearch" id="currentSearch" name="currentSearch" placeholder="{{ 'agreeGatewayV1App.templAttributeMapping.home.search' | translate }}">
                    <button class="input-group-addon btn btn-info" (click)="search(currentSearch)">
                        <span class="fa fa-search"></span>
                    </button>
                    <button class="input-group-addon btn btn-danger" (click)="clear()" *ngIf="currentSearch">
                        <span class="fa fa-trash-o"></span>
                    </button>
                </div>
            </form>
        </div>
    </div>
    <br/>
    <div class="table-responsive" *ngIf="templAttributeMappings">
        <table class="table table-striped">
            <thead>
            <tr jhiSort [(predicate)]="predicate" [(ascending)]="reverse" [callback]="transition.bind(this)">
            <th jhiSortBy="id"><span jhiTranslate="global.field.id">ID</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="jeTempId"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.jeTempId">Je Temp Id</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="attributeName"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.attributeName">Attribute Name</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="mappingType"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.mappingType">Mapping Type</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="value"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.value">Value</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="sourceViewColumnId"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.sourceViewColumnId">Source View Column Id</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="cretaedBy"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.cretaedBy">Cretaed By</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="lastUpdatedBy"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.lastUpdatedBy">Last Updated By</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="createdDate"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.createdDate">Created Date</span> <span class="fa fa-sort"></span></th>
            <th jhiSortBy="lastUpdatedDate"><span jhiTranslate="agreeGatewayV1App.templAttributeMapping.lastUpdatedDate">Last Updated Date</span> <span class="fa fa-sort"></span></th>
            <th></th>
            </tr>
            </thead>
            <tbody>
            <tr *ngFor="let templAttributeMapping of templAttributeMappings ;trackBy: trackId">
                <td><a [routerLink]="['../templ-attribute-mapping', templAttributeMapping.id ]">{{templAttributeMapping.id}}</a></td>
                <td>{{templAttributeMapping.jeTempId}}</td>
                <td>{{templAttributeMapping.attributeName}}</td>
                <td>{{templAttributeMapping.mappingType}}</td>
                <td>{{templAttributeMapping.value}}</td>
                <td>{{templAttributeMapping.sourceViewColumnId}}</td>
                <td>{{templAttributeMapping.cretaedBy}}</td>
                <td>{{templAttributeMapping.lastUpdatedBy}}</td>
                <td>{{templAttributeMapping.createdDate | date:'medium'}}</td>
                <td>{{templAttributeMapping.lastUpdatedDate | date:'medium'}}</td>
                <td class="text-right">
                    <div class="btn-group flex-btn-group-container">
                        <button type="submit"
                                [routerLink]="['../templ-attribute-mapping', templAttributeMapping.id ]"
                                class="btn btn-info btn-sm">
                            <span class="fa fa-eye"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.view">View</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'templ-attribute-mapping/'+ templAttributeMapping.id + '/edit'} }]"
                                replaceUrl="true"
                                class="btn btn-primary btn-sm">
                            <span class="fa fa-pencil"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.edit">Edit</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'templ-attribute-mapping/'+ templAttributeMapping.id + '/delete'} }]"
                                replaceUrl="true"
                                class="btn btn-danger btn-sm">
                            <span class="fa fa-remove"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.delete">Delete</span>
                        </button>
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
        <!-- Pager is not implemented yet, so this is normal pagination instead -->
        <div *ngIf="templAttributeMappings && templAttributeMappings.length">
            <div class="row justify-content-center">
                <jhi-item-count [page]="page" [total]="queryCount" [itemsPerPage]="itemsPerPage"></jhi-item-count>
            </div>
            <div class="row justify-content-center">
                <ngb-pagination [collectionSize]="totalItems" [(page)]="page"  (pageChange)="loadPage(page)"></ngb-pagination>
            </div>
        </div>
</div>
