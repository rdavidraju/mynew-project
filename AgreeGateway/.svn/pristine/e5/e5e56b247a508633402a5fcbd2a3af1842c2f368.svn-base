<div>
    <h2>
        <span jhiTranslate="agreeGatewayV1App.process.home.title">Processes</span>
        <button class="btn btn-primary float-right jh-create-entity create-process" [routerLink]="['/', { outlets: { popup: ['process-new'] } }]">
            <span class="fa fa-plus"></span>
            <span class="hidden-sm-down"  jhiTranslate="agreeGatewayV1App.process.home.createLabel">
            Create new Process
            </span>
        </button>
    </h2>
    <jhi-alert></jhi-alert>
    <div class="row">
        <div class="col-sm-12">
            <form name="searchForm" class="form-inline">
                <div class="input-group w-100 mt-3">
                    <input type="text" class="form-control" [(ngModel)]="currentSearch" id="currentSearch" name="currentSearch" placeholder="{{ 'agreeGatewayV1App.process.home.search' | translate }}">
                    <button class="input-group-addon btn btn-info" (click)="search(currentSearch)">
                        <span class="fa fa-search"></span>
                    </button>
                    <button class="input-group-addon btn btn-danger" (click)="clear()" *ngIf="currentSearch">
                        <span class="fa fa-trash-o"></span>
                    </button>
                </div>
            </form>
        </div>
    </div>
    <br/>
    <div class="table-responsive" *ngIf="processes">
        <table class="table table-striped">
            <thead>
            <tr>
            <th><span jhiTranslate="global.field.id">ID</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.processName">Process Name</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.startDate">Start Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.endDate">End Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.enabledFalg">Enabled Falg</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.projectId">Project Id</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.processCategory">Process Category</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.createdBy">Created By</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.lastUpdatedBy">Last Updated By</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.creationDate">Creation Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.process.lastUpdatedDate">Last Updated Date</span></th>
            <th></th>
            </tr>
            </thead>
            <tbody>
            <tr *ngFor="let process of processes ;trackBy: trackId">
                <td><a [routerLink]="['../process', process.id ]">{{process.id}}</a></td>
                <td>{{process.processName}}</td>
                <td>{{process.startDate | date:'mediumDate'}}</td>
                <td>{{process.endDate | date:'mediumDate'}}</td>
                <td>{{process.enabledFalg}}</td>
                <td>{{process.projectId}}</td>
                <td>{{process.processCategory}}</td>
                <td>{{process.createdBy}}</td>
                <td>{{process.lastUpdatedBy}}</td>
                <td>{{process.creationDate | date:'medium'}}</td>
                <td>{{process.lastUpdatedDate | date:'medium'}}</td>
                <td class="text-right">
                    <div class="btn-group flex-btn-group-container">
                        <button type="submit"
                                [routerLink]="['../process', process.id ]"
                                class="btn btn-info btn-sm">
                            <span class="fa fa-eye"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.view">View</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'process/'+ process.id + '/edit'} }]"
                                replaceUrl="true"
                                class="btn btn-primary btn-sm">
                            <span class="fa fa-pencil"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.edit">Edit</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'process/'+ process.id + '/delete'} }]"
                                replaceUrl="true"
                                class="btn btn-danger btn-sm">
                            <span class="fa fa-remove"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.delete">Delete</span>
                        </button>
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
