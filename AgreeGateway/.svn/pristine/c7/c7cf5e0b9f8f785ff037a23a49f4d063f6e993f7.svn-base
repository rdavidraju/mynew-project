
<div *ngIf="sourceProfiles">
    <h2><span jhiTranslate="agreeGatewayV1App.sourceProfiles.detail.title">Source Profiles</span> {{sourceProfiles.id}}</h2>
    <hr>
    <jhi-alert-error></jhi-alert-error>
    <dl class="row-md jh-entity-details">
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.sourceProfileName">Source Profile Name</span></dt>
        <dd>
            <span>{{sourceProfiles.sourceProfileName}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.description">Description</span></dt>
        <dd>
            <span>{{sourceProfiles.description}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.startDate">Start Date</span></dt>
        <dd>
            <span>{{sourceProfiles.startDate | date:'mediumDate'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.endDate">End Date</span></dt>
        <dd>
            <span>{{sourceProfiles.endDate | date:'mediumDate'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.enabledFlag">Enabled Flag</span></dt>
        <dd>
            <span>{{sourceProfiles.enabledFlag}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.connectionId">Connection Id</span></dt>
        <dd>
            <span>{{sourceProfiles.connectionId}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.tenantId">Tenant Id</span></dt>
        <dd>
            <span>{{sourceProfiles.tenantId}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.createdBy">Created By</span></dt>
        <dd>
            <span>{{sourceProfiles.createdBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.createdDate">Created Date</span></dt>
        <dd>
            <span>{{sourceProfiles.createdDate | date:'medium'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.lastUpdatedBy">Last Updated By</span></dt>
        <dd>
            <span>{{sourceProfiles.lastUpdatedBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.sourceProfiles.lastUpdatedDate">Last Updated Date</span></dt>
        <dd>
            <span>{{sourceProfiles.lastUpdatedDate | date:'medium'}}</span>
        </dd>
    </dl>

    <button type="submit"
            (click)="previousState()"
            class="btn btn-info">
        <span class="fa fa-arrow-left"></span>&nbsp;<span jhiTranslate="entity.action.back"> Back</span>
    </button>

    <button type="button"
            [routerLink]="['/', { outlets: { popup: 'source-profiles/'+ sourceProfiles.id + '/edit'} }]"
            replaceUrl="true"
            class="btn btn-primary">
        <span class="fa fa-pencil"></span>&nbsp;<span jhiTranslate="entity.action.edit"> Edit</span>
    </button>
</div>
