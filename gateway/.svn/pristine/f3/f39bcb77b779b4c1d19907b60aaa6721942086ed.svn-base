<div class="component-title">
	<div class="col-md-6 col-sm-6 col-xs-6">
		<h4>File Templates</h4>
		<div ng2FileDrop [ngClass]="{'nv-file-over': hasBaseDropZoneOver}" (fileOver)="fileOverBase($event)" (onFileDrop)="onFileDrop($event)"
			(click)="singleFile.click()" [uploader]="uploader" class="well my-drop-zone hand" mdTooltip="Click or Drop Files Here"
			style="width: 20% !important;">
			   

			   <i class="material-icons cloudupload" *ngIf="!uploader.queue || (uploader.queue && uploader.queue.length==0 )">cloud_upload</i>
			   <i class="material-icons cloudsuccess" *ngIf="uploader.queue && uploader.queue.length>0">cloud_done</i>
				</div>
	</div>
	<div class="col-md-6 col-sm-6 col-xs-6 ">
		<button md-icon-button (click)="ftDT.exportCSV()" class=" hand pull-right" mdTooltip="Export templates">
			<i class="fa fa-file-excel-o fa-1x"></i>
		</button>


		<button md-icon-button [routerLink]="['/file-templates', {outlets: { 'content': ['filetemplate-new']}}]" class="pull-right"
		 mdTooltip="Add New Template" *jhiHasAnyAuthority="['FILE_TEMPLATE_CREATE']">
			<md-icon>add</md-icon>
		</button>
		<!-- <button md-icon-button (click)="addNewTemplate()" 
						class="pull-right   md-36" mdTooltip="Add New Template" ><md-icon>add</md-icon></button> -->
		<!-- <button md-icon-button class="pull-right search-icon-body"  mdTooltip="Search">
				<md-icon>search</md-icon>
			</button> -->
		<!-- 	
			<img class="pull-right" alt="export" src="../../home/media/exportAsCSV.png" height="24" width="24">  -->
		<!-- <i class="pull-right material-icons md-36 hand ripple search-icon-body ripple">search</i> -->
		<!-- <md-input-container class="pull-right hidethis">
				<input class="mySearchBox" mdInput placeholder="What're we looking for?" [(ngModel)] = "fileTemplatesService.searchData" (keyup.enter)="searchText();"> </md-input-container>
				 -->

	</div>
</div>
<div class="component-body">

	<div class="position-relative">
		
		<!-- scrollHeight="calc(100vh - 315px)" -->
		<div class="list-table table-responsive">
			<table>
				<thead>
<tr>
		<th>
					refId
				</th>
	<th *ngFor="let col of lookUpCodeService.headers;let i=index">
		{{col}}
	</th>
</tr>
				</thead>
				<tbody>
					<tr *ngFor="let row of lookUpCodeService.result;let i=index">
						<td>
						{{	row['refId']}}
						</td>
						<td *ngFor="let key of row  | keys ;let j=index">
						{{key.value}}
						</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
</div>