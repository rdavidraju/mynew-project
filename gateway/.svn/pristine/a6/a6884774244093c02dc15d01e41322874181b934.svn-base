<div class="sub-side-navbar datViewSidePad">
	<perfect-scrollbar style="height: auto; width:100%; overflow-x: auto;">
		<p-accordion>
			<p-accordionTab header="{{col.sourceName}}  ({{col.count}})" [selected]="col.firstOpened" *ngFor="let col of journalsBatchSideBarList; let i = index">
				<!-- style="height: auto; width:100%; overflow-x: auto;max-height:300px;" -->
				<perfect-scrollbar [class]="getInSidHeight()">
					<ul style="padding-left: 13px;">
						<li *ngFor="let col of journalsBatchSideBarList[i].Lists; let j = index">
							<div class="selecteditem hand ripple" [routerLink]="['/journals-header-data', {outlets: {'content': col.id+'/details'}}]">
								<span>{{col.jeBatchName}}</span>
							</div>
						</li>
					</ul>
				</perfect-scrollbar>
			</p-accordionTab>
		</p-accordion>
	</perfect-scrollbar>
</div>