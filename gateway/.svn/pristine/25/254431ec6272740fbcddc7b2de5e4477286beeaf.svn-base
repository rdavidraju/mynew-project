<div>
    <h2>
        <span jhiTranslate="agreeGatewayV1App.project.home.title">Projects</span>
        <button class="btn btn-primary float-right jh-create-entity create-project" [routerLink]="['/', { outlets: { popup: ['project-new'] } }]">
            <span class="fa fa-plus"></span>
            <span class="hidden-sm-down"  jhiTranslate="agreeGatewayV1App.project.home.createLabel">
            Create new Project
            </span>
        </button>
    </h2>
    <jhi-alert></jhi-alert>
    <div class="row">
        <div class="col-sm-12">
            <form name="searchForm" class="form-inline">
                <div class="input-group w-100 mt-3">
                    <input type="text" class="form-control" [(ngModel)]="currentSearch" id="currentSearch" name="currentSearch" placeholder="{{ 'agreeGatewayV1App.project.home.search' | translate }}">
                    <button class="input-group-addon btn btn-info" (click)="search(currentSearch)">
                        <span class="fa fa-search"></span>
                    </button>
                    <button class="input-group-addon btn btn-danger" (click)="clear()" *ngIf="currentSearch">
                        <span class="fa fa-trash-o"></span>
                    </button>
                </div>
            </form>
        </div>
    </div>
    <br/>
    <div class="table-responsive" *ngIf="projects">
        <table class="table table-striped">
            <thead>
            <tr>
            <th><span jhiTranslate="global.field.id">ID</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.tenantId">Tenant Id</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.projectName">Project Name</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.startDate">Start Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.endDate">End Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.enabledFlag">Enabled Flag</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.createdBy">Created By</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.lastUpdatedBy">Last Updated By</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.creationDate">Creation Date</span></th>
            <th><span jhiTranslate="agreeGatewayV1App.project.lastUpdatedDate">Last Updated Date</span></th>
            <th></th>
            </tr>
            </thead>
            <tbody>
            <tr *ngFor="let project of projects ;trackBy: trackId">
                <td><a [routerLink]="['../project', project.id ]">{{project.id}}</a></td>
                <td>{{project.tenantId}}</td>
                <td>{{project.projectName}}</td>
                <td>{{project.startDate | date:'mediumDate'}}</td>
                <td>{{project.endDate | date:'mediumDate'}}</td>
                <td>{{project.enabledFlag}}</td>
                <td>{{project.createdBy}}</td>
                <td>{{project.lastUpdatedBy}}</td>
                <td>{{project.creationDate | date:'medium'}}</td>
                <td>{{project.lastUpdatedDate | date:'medium'}}</td>
                <td class="text-right">
                    <div class="btn-group flex-btn-group-container">
                        <button type="submit"
                                [routerLink]="['../project', project.id ]"
                                class="btn btn-info btn-sm">
                            <span class="fa fa-eye"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.view">View</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'project/'+ project.id + '/edit'} }]"
                                replaceUrl="true"
                                class="btn btn-primary btn-sm">
                            <span class="fa fa-pencil"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.edit">Edit</span>
                        </button>
                        <button type="submit"
                                [routerLink]="['/', { outlets: { popup: 'project/'+ project.id + '/delete'} }]"
                                replaceUrl="true"
                                class="btn btn-danger btn-sm">
                            <span class="fa fa-remove"></span>
                            <span class="hidden-md-down" jhiTranslate="entity.action.delete">Delete</span>
                        </button>
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
