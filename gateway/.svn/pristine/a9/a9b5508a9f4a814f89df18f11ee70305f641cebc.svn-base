
<div *ngIf="approvalGroupMembers">
    <h2><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.detail.title">Approval Group Members</span> {{approvalGroupMembers.id}}</h2>
    <hr>
    <jhi-alert-error></jhi-alert-error>
    <dl class="row-md jh-entity-details">
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.groupId">Group Id</span></dt>
        <dd>
            <span>{{approvalGroupMembers.groupId}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.seq">Seq</span></dt>
        <dd>
            <span>{{approvalGroupMembers.seq}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.userId">User Id</span></dt>
        <dd>
            <span>{{approvalGroupMembers.userId}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.startDate">Start Date</span></dt>
        <dd>
            <span>{{approvalGroupMembers.startDate | date:'mediumDate'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.endDate">End Date</span></dt>
        <dd>
            <span>{{approvalGroupMembers.endDate | date:'mediumDate'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.createdBy">Created By</span></dt>
        <dd>
            <span>{{approvalGroupMembers.createdBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.createdDate">Created Date</span></dt>
        <dd>
            <span>{{approvalGroupMembers.createdDate | date:'medium'}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.lastUpdatedBy">Last Updated By</span></dt>
        <dd>
            <span>{{approvalGroupMembers.lastUpdatedBy}}</span>
        </dd>
        <dt><span jhiTranslate="agreeGatewayV1App.approvalGroupMembers.lastUpdatedDate">Last Updated Date</span></dt>
        <dd>
            <span>{{approvalGroupMembers.lastUpdatedDate | date:'medium'}}</span>
        </dd>
    </dl>

    <button type="submit"
            (click)="previousState()"
            class="btn btn-info">
        <span class="fa fa-arrow-left"></span>&nbsp;<span jhiTranslate="entity.action.back"> Back</span>
    </button>

    <button type="button"
            [routerLink]="['/', { outlets: { popup: 'approval-group-members/'+ approvalGroupMembers.id + '/edit'} }]"
            replaceUrl="true"
            class="btn btn-primary">
        <span class="fa fa-pencil"></span>&nbsp;<span jhiTranslate="entity.action.edit"> Edit</span>
    </button>
</div>
