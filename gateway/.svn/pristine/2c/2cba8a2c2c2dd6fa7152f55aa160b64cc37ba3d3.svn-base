<p-dialog header="Re-Assign" [(visible)]="reAssignTo" [resizable]="true" [responsive]="true" [dismissableMask]="false">
    <div class="col-md-12" style="display: flex;">
        <div class="col-md-6" style="white-space:nowrap;top: 7px;">Reassign To :</div>
        <div class="col-md-6">
            <md-select placeholder="" name="userslists" [ngClass]="{'color-black' : isViewOnly}" [(ngModel)]="selectedUser" style="padding-top: 0px;">
                <md-option *ngFor="let col of usersList;" [value]="col.id">
                    {{col.firstName}} {{col.lastName}}
                </md-option>
            </md-select>
        </div>
    </div>
    <div style="margin-top: 10px;">
        <button *ngIf="selectedUser" md-button style="float: right;" (click)="reassignSelectedBatch();reAssignTo = true;">Ok</button>
        <button md-button style="float: right;" (click)="reAssignTo = false;">Cancel</button>
    </div>
</p-dialog>
