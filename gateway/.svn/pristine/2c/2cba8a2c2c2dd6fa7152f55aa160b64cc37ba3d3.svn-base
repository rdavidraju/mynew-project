<div class="target-data approvalWQ">
    <p-dialog header="Transaction Details" [(visible)]="display" [resizable]="true" [responsive]="true">
        <div class="row" style="margin-left: -4px;">
            <div class="col-md-4 col-xs-6 form-group">
                <md-input-container class="example-full-width ">
                    <input mdInput [(ngModel)]="selectedBatchDetails.notificationName" [ngClass]="{'color-black' : isViewOnly}" placeholder="Notification Name"
                        name="notificationName" #notificationName="ngModel" readonly>
                </md-input-container>
            </div>
            <div class="col-md-4 col-xs-6 form-group">
                <md-input-container class="example-full-width ">
                    <input mdInput [(ngModel)]="selectedBatchDetails.ruleName" [ngClass]="{'color-black' : isViewOnly}" placeholder="Rule Name"
                        name="ruleName" #ruleName="ngModel" readonly>
                </md-input-container>
            </div>
            <div class="col-md-4 col-xs-6 form-group">
                <md-input-container class="example-full-width ">
                    <input mdInput [(ngModel)]="selectedBatchDetails.dataViewName" [ngClass]="{'color-black' : isViewOnly}" placeholder="Data View Name"
                        name="dataViewName" #dataViewName="ngModel" readonly>
                </md-input-container>
            </div>
            <!-- <div class="col-md-3 col-xs-6 form-group">
                <md-input-container class="example-full-width ">
                    <input mdInput [(ngModel)]="selectedBatchDetails.status" [ngClass]="{'color-black' : isViewOnly}" placeholder="Current Status"
                        name="status" #status="ngModel" readonly>
                </md-input-container>
            </div> -->
        </div>
        <!-- <div class="row" style="margin-left: -4px;border-bottom: 1px solid #eeebeb;">
