            <div class="d-flex justify-content-between sr-pg-con">
                <div class="d-flex">
                    <input type="text" #gb placeholder="Search.." class="form-control search-input mr-2">
                    <div class="customToggleTabs" style="margin: auto;">
                        <!-- *ngIf="selectedBatchRows.length<1" -->
                        <md-button-toggle-group #group="mdButtonToggleGroup" (change)="changeAppType($event);" [value]="selectedProcessType">
                            <md-button-toggle value="IN_PROCESS">
                                In Process
                            </md-button-toggle>
                            <md-button-toggle value="APPROVED">
                                Approved
                            </md-button-toggle>
                            <md-button-toggle value="REJECTED">
                                Rejected
                            </md-button-toggle>
                            <md-button-toggle value="All">
                                All
                            </md-button-toggle>
                        </md-button-toggle-group>
                    </div>
                </div>
                <!-- <md-paginator *ngIf="batchListRecordsLength>0 && selectedBatchRows.length<1" [length]="batchListRecordsLength" [pageSize]="itemsPerPage"
