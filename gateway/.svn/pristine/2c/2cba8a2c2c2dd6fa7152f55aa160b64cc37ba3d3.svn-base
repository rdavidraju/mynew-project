                </md-paginator> -->
                <div *ngIf="selectedBatchRows.length>0 && selectedProcessType == 'IN_PROCESS'">
                    <button class="action-button hov-sweep-to-right" md-raised-button color="primary" (click)="approveSelectedBatch();">
                        <md-icon class="approvalActionIcons">thumb_up</md-icon>
                        <span>Approve</span>
                    </button>
                    <button class="action-button hov-sweep-to-right" md-raised-button color="primary" (click)="rejectSelectedBatch();">
                        <md-icon class="approvalActionIcons">thumb_down</md-icon>
                        <span>Reject</span>
                    </button>
                    <button class="action-button hov-sweep-to-right" md-raised-button color="primary" (click)="reAssignTo = true;">
                        <md-icon class="approvalActionIcons">redo</md-icon>
                        <span>Reassign</span>
                    </button>
                </div>
            </div>
            <div class="position-relative">
                <div class="show-hide-class">
                    <p-multiSelect [options]="columnOptions" [(ngModel)]="notificationBatchTableColumns" title="Show/Hide Columns"></p-multiSelect>
                </div>
                <div class="list-table checkBoxCls">
                    <p-dataTable [value]="notificationBatches" reorderableColumns="true" resizableColumns="true" columnResizeMode="expand" scrollable="true"
                        [responsive]="true" [headerCheckboxToggleAllPages]="true" [globalFilter]="gb" #dt [rows]="15" [paginator]="false"
                        [pageLinks]="3" [rowsPerPageOptions]="[5,10,20,50]" [(selection)]="selectedBatchRows">
                        <!-- <p-column  [style]="{'width':'38'}" [sortable]="true">
