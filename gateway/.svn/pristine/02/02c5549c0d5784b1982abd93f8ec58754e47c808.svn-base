/* ==========================================================================
Styles for Search Box
========================================================================== */


.source_search {
  height: 50px;
  font-size: 15px;
  font-style: italic;
  display: inline-block;
  font-weight: 100;
  font-family: "Lato";
  border: none;
  outline: none;
  padding: 3px;
  padding-right: 40px;
  width: 0px;
  position: absolute;
  top: 0;
  right: 0;
  background: none;
  z-index: 3;
  transition: width .4s cubic-bezier(0.000, 0.795, 0.000, 1.000);
  cursor: pointer;
}


.source_search:focus {
