.rule-paginator{
	display: none;
	overflow:auto;
	width: 0;
	&.p-active {
		display: block;
	}
	&.p-animate {
		width: 100%;
		transition: all 1s ease-in-out;
	}
}
.search-show-styles{
	transition: all 2s ease-in-out;
}


.traslate-search-up{
    transform: translateY(-153%);
    transition: 0.5s;
    margin-left: 146px;
    z-index: 3;
	transition-timing-function: ease-in;
	position:fixed;

}
.traslate-search-down{
	transform: translateY(0%);
    transition: 0.5s;
    margin-left: 0px;
    transition-timing-function: ease-out;
	

}
.translate-paginator{
	transform: translateY(780px);
    transition: 1s;
    /* margin-left: 146px; */
    z-index: 3;
    /* margin-right: 0px; */
    transition-timing-function: ease-in;
}

