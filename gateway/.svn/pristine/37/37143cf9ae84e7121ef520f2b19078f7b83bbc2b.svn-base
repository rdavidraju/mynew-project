<div class="split-example" style="min-height: calc(100vh - 60px);">
    <div style="height: 100%;width:100%">


        <div class="component-body dashboardV8">
            <div id="dashboardTabSticky">
                <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12 dashTabStyle">
                    <!-- padding: 10px; -->
                    <div class="col-lg-3 col-md-3 col-sm-6 mainNav customToggleTabs" style=" padding-top: 14px;">
                        <md-button-toggle-group (change)="changeModuleType($event);">
                            <md-button-toggle value="Reconciliation" checked> Reconciliation </md-button-toggle>
                            <md-button-toggle value="Accounting"> Accounting </md-button-toggle>
                        </md-button-toggle-group>
                    </div>
                    <div class="col-md-8 col-sm-9 col-xs-11 push-top customToggleTabs" style="margin: auto;">
                        <!--  style="overflow-x: hidden;" -->
                        <div id="daterange" *ngIf="custompicker" class="row " [@enterAnimation]>
                            <!--  style=" margin-top: 15px;" -->
                            <!-- blockSlideFromTop -->
                            <!-- <date-range-picker [class]="'col-md-12 col-sm-12 form-control'"  [options]="daterangepickerOptions" (rangeSelected)="rangeSelected($event)" >
                            </date-range-picker> -->

                            <div class="col-md-9" style="margin: auto;">
                                <!-- style="text-align: right;" -->
                                <div class="datedisplay" style="width: 404px;">
                                    <md-button-toggle-group #group="mdButtonToggleGroup" (change)="toggleRange($event);">
                                        <md-button-toggle value='Today' mdTooltip="1 Day"> 1D </md-button-toggle>
                                        <md-button-toggle value='Last 7 Days' mdTooltip="1 Week"> 1W </md-button-toggle>
                                        <md-button-toggle value='Last 14 Days' mdTooltip="2 Weeks"> 2W </md-button-toggle>
                                        <md-button-toggle value='Last 30 Days' mdTooltip="1 Month">
                                            1M </md-button-toggle>
                                        <md-button-toggle value='Last 2Months' mdTooltip="2 Months">
                                            2M </md-button-toggle>
                                        <md-button-toggle value='Last 3Months' mdTooltip="3 Months">
                                            3M </md-button-toggle>
                                        <md-button-toggle value='Last 6Months' mdTooltip="6 Months">
                                            6M </md-button-toggle>
                                        <md-button-toggle value='thismonth' checked mdTooltip="This Month">
                                            This Month </md-button-toggle>
                                        <!-- <md-button-toggle value='Custom' mdTooltip="Select Daterange" (click)="stpicker.open()">
                                    Custom </md-button-toggle> -->
                                    </md-button-toggle-group>
                                </div>
                            </div>
                            <div class="col-md-3 navStEndPicker" style="display: flex;padding-top: 3px;">
                                <div class="col-md-6 stDtcol" style="margin: auto;">
                                    <md-form-field>
                                        <input mdInput [mdDatepicker]="stpickers" placeholder="From" [disabled]="!selectedMoreRange" [(ngModel)]="reconDateRange.startDate"
                                            name="startDate" (dateChange)="newrequest = true;isStartDateChanged = true;" [max]="maxDate"
                                            (dateChange)="getmindate();dateFormatRange = 'Custom';refreshData();">
                                        <!-- (ngModelChange)="edpickers.open()" (dateInput)="getmindate()" (click)="stpickers.open()"-->
                                        <md-datepicker-toggle mdSuffix [for]="stpickers" [disabled]="!selectedMoreRange"></md-datepicker-toggle>
                                        <md-datepicker #stpickers></md-datepicker>
                                    </md-form-field>
                                </div>
                                <div class="col-md-6" style="margin: auto;">
                                    <md-form-field>
                                        <input mdInput [mdDatepicker]="edpickers" placeholder="To" [disabled]="!selectedMoreRange" [max]="maxDate" [min]="minDate"
                                            [(ngModel)]="reconDateRange.endDate" name="endDate" (dateChange)="newrequest = true;isEndDateChanged = true;dateFormatRange = 'Custom';refreshData();">
                                        <md-datepicker-toggle mdSuffix [for]="edpickers" [disabled]="!selectedMoreRange"></md-datepicker-toggle>
                                        <md-datepicker #edpickers></md-datepicker>
                                    </md-form-field>
                                </div>
                            </div>
                        </div>
                        <md-tab-group class="year-det " style="margin-left: auto;margin-right: 0;" *ngIf="!custompicker" [(selectedIndex)]="selectedPeriodTab"
                            (focusChange)="cancelRequest();changePeriod($event);" [@enterAnimation]>
                            <!-- blockSlideFromTop -->
                            <span>
                                <md-tab *ngFor="let month of monthslist" label="{{month.periodName}}"></md-tab>
                            </span>
                        </md-tab-group>
                    </div>
                    <div class="col-md-1 col-sm-1 col-xs-1 text-center" style="padding: 14px;">
                        <i class="fa fa-remove hand" *ngIf="custompicker" (click)="closePicker()" style="padding: 0px 5px;"></i>
                        <i class="fa fa-calendar hand" *ngIf="!custompicker" (click)="openPicker()" style="padding: 0px 5px;"></i>
                        <i class="fa fa-refresh hand refreshBut" (click)="cancelRequest();refreshData();" mdTooltip="Refresh" style="padding: 0px 5px;"></i>
                    </div>
                </div>
            </div>
            <div id="reconciliationDiv" class="reconview matTabPainationStyle" *ngIf="showrecon" [@enterAnimation]>
                <div>
                    <div class="cat-heading">
                        <span>Reconciliation Process
                            <span *ngIf="dateFormatRange == 'Period Wise'">For Month</span>
                            <span *ngIf="dateFormatRange == 'Range Wise'">For Period</span>
                            <span *ngIf="dateFormatRange == 'Custom'">From Period</span>
                            <span class="selectedLabel" *ngIf="dateFormatRange != 'Custom';">
                                {{selectedMonth}}
                            </span>
                            <span class="selectedLabel" *ngIf="dateFormatRange == 'Custom';">
                                {{reconDateRange.startDate | date: 'mediumDate'}} To {{reconDateRange.endDate | date: 'mediumDate'}}
                            </span>
                        </span>
                    </div>
                    <div *ngIf="reconAllData && !reconAllData.length">
                        <span style="padding-left: 15px;font-size: 15px;color: #4688F4;" class="selectedLabel">No activity for selected period!
                            <!-- <span class="selectedLabel" style="font-size: 15px;color: #4688F4;">
                        {{selectedMonth}}
                    </span>! --></span>
                    </div>
                    <div class="reconview-list hide-mat-tab toggleTabs" *ngIf="reconAllData && reconAllData.length">
                        <md-tab-group class="" [mode]="stacked" [(selectedIndex)]="selectedReconTab" dynamicHeight="true" (focusChange)="cancelRequest();displayReconStatus($event.index);"
                            fxFlex md-stretch-tabs>
                            <md-tab *ngFor="let reconrg of reconAllData;let spInd=index;">
                                <ng-template md-tab-label>
                                    <div class="mat-sml-btn ellipsis-text elementToAnimateOpacity" style="width:100%;margin-right: 10px;justify-content:center;">
                                        <div class="flex-column-center" style="overflow: hidden;">
                                            <div>
                                                <span class="ellipsis-text processname" mdTooltip="{{reconrg.ruleGroupName}}" mdTooltipShowDelay="1000">{{reconrg.ruleGroupName}}</span>

                                                <span class="agre-value" mdTooltip="Reconciled Count" matTooltipShowDelay="300" [ngStyle]="{'color':reconrg.recon.count > 0 ? 'orange' : 'gray' }">{{reconrg.recon.countPer}}%
                                                    <span class="agre-pretext" [ngStyle]="{'color':'gray' }">({{reconrg.recon.count | number: '.0'}})</span>
                                                    <!-- <span class="agre-pretext" [ngStyle]="{'color':'gray' }">(Reconciled Count)</span> -->
                                                </span>
                                                <span *ngIf="reconrg.multiCurrency != 'true'" class="agre-value" mdTooltip="Reconciled Amount" matTooltipShowDelay="300"
                                                    [ngStyle]="{'color':reconrg.recon.amount > 0 ? 'green' : 'gray' }">
                                                    <span>|</span> {{reconrg.recon.amountPer}}%
                                                    <span *ngIf="reconrg.multiCurrency != 'true'" class="agre-pretext" [ngStyle]="{'color':'gray' }">({{reconrg.recon.amount | currency : reconrg.currencyCode[0] : 'symbol-narrow':'1.2-2'}})</span>
                                                </span>

                                                <!-- <span *ngIf="reconrg.multiCurrency != 'true'" class="agre-pretext" [ngStyle]="{'color':'gray' }">(Reconciled Amount)</span> -->
                                            </div>
                                        </div>
                                    </div>
                                </ng-template>
                            </md-tab>
                        </md-tab-group>
                    </div>
                </div>
                <div class="viewComb-list hide-mat-tab toggleTabs" *ngIf="reconAllData && reconAllData.length">
                    <div class="cat-heading">
                        <span>Sub Process Analysis For
                            <span class="selectedLabel">
                                {{reconAllData[selectedReconTab].ruleGroupName}}
                            </span>
                        </span>
                        <!-- {{subProcessAnalysisHeading}} -->
                    </div>
                    <md-tab-group [(selectedIndex)]="selectedReconAnalysis" (focusChange)="changeReconViewTab($event);" disableRipple>
                        <!-- <md-tab *ngIf="loadingTab">
                    <ng-template md-tab-label>
                        <div>
                            <div class="d-flex">
                                <div>
                                    <p class="m-0" style="font-size: 12px;font-weight: normal;">Reconciliation For</p>
                                    <div class="d-flex align-items-center">
                                        <i class="material-icons bgicon">insert_drive_file</i>
                                        <div>
                                            <div class="d-block loadingTabName"></div>
                                            <div class="d-block loadingTabPer"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="switch-icon">
                                    <i class="material-icons">compare_arrows</i>
                                </div>
                                <div>
                                    <p class="m-0" style="font-size: 12px;font-weight: normal;">Reconciliation With</p>
                                    <div class="d-flex align-items-center" style="padding-right:7px;">
                                        <i class="material-icons bgicon">insert_drive_file</i>
                                        <div>
                                            <div class="d-block loadingTabName"></div>
                                            <div class="d-block loadingTabPer"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </ng-template>
                </md-tab> -->
                        <md-tab *ngFor="let col of reconViewCombinationSpecificAnalysis;let i=index;">
                            <ng-template md-tab-label>
                                <!-- *ngIf="!loadingTab" -->
                                <div [ngClass]="{'disablePointerEvents':loadingKPIGrp == true}" style="    width: 100%;">
                                    <!-- [ngStyle]="{'pointer-events':loadingKPIGrp == true ? 'green' : 'red' }" -->
                                    <!--  (click)="fetchReconKPIAndDetails(col.combination[0].viewId, col.combination[0].DisplayviewType, col.combination[0].viewType, col.combination[0].viewName,'Reconciled');
                        fetchReconKPIAndDetailsTrg(col.combination[1].viewId, col.combination[1].DisplayviewType, col.combination[1].viewType, col.combination[1].viewName, 'Reconciled');" -->
                                    <!-- <h6 class="m-0">Reconcile</h6> -->
                                    <div class="d-flex" style="height: 16px;padding: 0px 5px;">
                                        <span class="col-md-5" style="text-align: left;">
                                            <p class="m-0" style="font-size: 10px;font-weight: normal;">
                                                Reconciliation For
                                            </p>
                                        </span>
                                        <span class="col-md-2"></span>
                                        <span class="col-md-5" style="text-align: left;">
                                            <p class="m-0" style="font-size: 10px;font-weight: normal;">
                                                Reconciliation With
                                            </p>
                                        </span>
                                    </div>
                                    <div class="d-flex" style="padding: 0px 5px;">
                                        <div class="col-md-5 srcReconOrder_{{i}} topBottomMargin" style="order:0">
                                            <!--  (click)="changeReconViewOrder = !changeReconViewOrder;fetchReconKPIAndDetails(col.combination[0].viewId, col.combination[0].DisplayviewType, col.combination[0].viewType, col.combination[0].viewName,'Reconciled');
                                toggleReconView = true;" -->
                                            <!-- <p class="m-0" style="font-size: 12px;font-weight: normal;">Reconciliation For</p> -->
                                            <div class="d-flex align-items-center" mdTooltip="{{col.combination[0].viewName}}">
                                                <!-- <i class="material-icons bgicon">insert_drive_file</i> -->
                                                <div class="ellipsis-text">
                                                    <span *ngIf="toggleReconView && selectedReconAnalysis == i" class="d-block ellipsis-text">{{col.combination[0].viewName}}</span>
                                                    <!-- *ngIf="toggleReconView && selectedReconAnalysis == i" -->
                                                    <p *ngIf="!toggleReconView && selectedReconAnalysis == i" class="d-block ellipsis-text" style="margin-bottom: 0;">{{col.combination[0].viewName}}</p>
                                                    <p *ngIf="selectedReconAnalysis != i" class="d-block ellipsis-text" style="margin-bottom: 0;">{{col.combination[0].viewName}}</p>
                                                    <!-- <p *ngIf="!toggleReconView && selectedReconAnalysis == i" class="d-block" style="margin-bottom: 0;">{{col.combination[0].viewName}}</p> -->
                                                    <span class="d-block ellipsis-text" *ngIf="toggleReconView && selectedReconAnalysis == i" [@enterAnimation]>{{col.combination[0].countPer | amountFormat:'': 2}}%</span>
                                                    <!-- <span class="d-block" *ngIf="showReconApprovalPer && toggleReconView && selectedReconAnalysis == i" [@enterAnimation]>{{col.combination[0].unAppCtPer | amountFormat:'': 2}}%</span> -->
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 switch-icon topBottomMargin" style="order:1">
                                            <i class="material-icons hand" (click)="changeReconViewOrder = !changeReconViewOrder;changeReconViewTab();">compare_arrows</i>
                                        </div>
                                        <div class="col-md-5 trgReconOrder_{{i}} topBottomMargin" style="order:3">
                                            <!--  (click)="changeReconViewOrder = !changeReconViewOrder;fetchReconKPIAndDetails(col.combination[1].viewId, col.combination[1].DisplayviewType, col.combination[1].viewType, col.combination[1].viewName, 'Reconciled');
                                toggleReconView = false;" -->
                                            <!-- <p class="m-0" style="font-size: 12px;font-weight: normal;">Reconciliation With</p> -->
                                            <div class="d-flex align-items-center" style="padding-right:7px;" mdTooltip="{{col.combination[1].viewName}}">
                                                <!-- <i class="material-icons bgicon">insert_drive_file</i> -->
                                                <div class="ellipsis-text">
                                                    <span class="d-block ellipsis-text" *ngIf="!toggleReconView && selectedReconAnalysis == i" style="margin-bottom: 0;">{{col.combination[1].viewName}}</span>
                                                    <!-- *ngIf="toggleReconView && selectedReconAnalysis == i" -->
                                                    <p class="d-block ellipsis-text" *ngIf="toggleReconView && selectedReconAnalysis == i" style="margin-bottom: 0;">{{col.combination[1].viewName}}</p>
                                                    <p class="d-block ellipsis-text" *ngIf="selectedReconAnalysis != i" style="margin-bottom: 0;">{{col.combination[1].viewName}}</p>
                                                    <!--   <span class="d-block" *ngIf="!toggleReconView && selectedReconAnalysis == i">{{col.combination[1].viewName}}</span> -->
                                                    <span class="d-block ellipsis-text" *ngIf="!toggleReconView && selectedReconAnalysis == i" [@enterAnimation]>{{col.combination[1].countPer | amountFormat:'': 2}}%</span>
                                                    <!-- <span class="d-block" *ngIf="showReconApprovalPer && !toggleReconView && selectedReconAnalysis == i" [@enterAnimation]>{{col.combination[0].unAppCtPer | amountFormat:'': 2}}%</span> -->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </ng-template>
                        </md-tab>
                    </md-tab-group>
                </div>
                <div class="reconGrids tabPaddingZero hide-mat-tab kpiHeaderFnt toggleTabs" *ngIf="reconAllData && reconAllData.length" id="reconDetailAnalysis">
                    <div class="cat-heading">
                        <span>KPI's For
                            <span *ngIf="reconAllData" style="margin-left: 0px;font-weight: 600 !important;">{{reconSelViewName}}</span>
                            <!-- {{reconAllData[selectedReconTab].ruleGroupName}} -->
                        </span>
                    </div>
                    <md-tab-group class="prof-det profSrc" [mode]="stacked" [(selectedIndex)]="selectedTab" dynamicHeight="true" fxFlex md-stretch-tabs>
                        <md-tab>
                            <ng-template md-tab-label>
                                <div [ngClass]="{selectedModule: currentModule == 'Reconciliation Status'}" (click)="showActiveCls('src');showData('Reconciliation Status')"
                                    style="padding-left: 0px;">
                                    <div id="headerTab">
                                        <div class="kpiHeaderFont">
                                            <span>Reconciliation Status</span>
                                        </div>
                                        <div style="position: absolute;right: 4px;top: 9px;">
                                            <span style="visibility:hidden;" (click)="openSideBar('Reconciliation Status');" *ngIf="reconPeriodAnalysisType == 'None' && !showReconDrillDownData;">
                                                <i class="material-icons" style="font-size: 16px !important;">settings</i>
                                            </span>
                                            <span *ngIf="reconPeriodAnalysisType != 'None' && reconPeriodAnalysisType != 'Months' && showReconDrillDownData && reconDateRanges && reconDateRanges.length>1"
                                                (click)="fetchReconDrillDown('back');">
                                                <i class="material-icons" style="font-size: 16px !important;">reply</i>
                                            </span>
                                            <!-- <button class="headerbutton" md-icon-button style="visibility:hidden;" (click)="openSideBar('Reconciliation Status');"
                                        *ngIf="reconPeriodAnalysisType == 'None' && !showReconDrillDownData;">
                                        <md-icon style="font-size: 16px !important;">settings</md-icon>
                                    </button> -->
                                            <!-- <button *ngIf="reconPeriodAnalysisType != 'None' && reconPeriodAnalysisType != 'Months' && showReconDrillDownData && reconDateRanges && reconDateRanges.length>1"
                                        class="headerbutton" md-icon-button (click)="fetchReconDrillDown('back');">
                                        <md-icon style="font-size: 16px !important;">reply</md-icon>
                                    </button> -->
                                        </div>
                                    </div>
                                    <div class="reconBackgroundColor" style="text-align: -webkit-center;height: 210px;width: 250px;" id="reconStatusTest">
                                        <canvas *ngIf="showRecon && pieChartReconciliation && pieChartReconciliationLabels && !showReconDrillDownData" baseChart
                                            [data]="pieChartReconciliation" [labels]="pieChartReconciliationLabels" [chartType]="pieChartReconciliationType"
                                            [options]="pieChartReconciliationOptions" [colors]="pieChartReconciliationColors"
                                            (chartClick)="fetchReconCombViews('Reconciled', $event);fetchReconDrillDown();scrollTab();">
                                            <!-- fetchReconciliationStatus($event); -->
                                        </canvas>
                                        <canvas *ngIf="showReconDrillDownData" baseChart [datasets]="reconChartData" [labels]="reconChartLabels" [options]="reconChartOptions"
                                            [colors]="reconChartColors" [legend]="reconChartLegend" [chartType]="reconChartType"
                                            (chartClick)="fetchReconciliationStatus($event);fetchGrpByData('Reconciled',$event);reconSrcBlock = true;reconTrgBlock = false;showActiveCls('src');scrollTab();"></canvas>
                                        <!-- fetchReconCombViews('reconciliation', $event); -->
                                    </div>
                                </div>
                            </ng-template>
                        </md-tab>
                        <md-tab>
                            <ng-template md-tab-label *ngIf="reconAllData[selectedReconTab].multiCurrency == 'true'">
                                <div class="spinnerCurrencyBlock"></div>
                                <div>
                                    <div id="headerTab">
                                        <div class="kpiHeaderFont">Un-Reconciled By Currency</div>
                                        <div style="position: absolute; right: 3px;top: 12px;">
                                            <span>
                                                <i *ngIf="pieChartActivityAccountingType == 'pie'" class="material-icons" (click)="pieChartActivityAccountingType = 'doughnut';"
                                                    mdTooltip="Donut Chart" style="font-size: 16px;">donut_small</i>
                                                <i *ngIf="pieChartActivityAccountingType == 'doughnut'" class="material-icons" (click)="pieChartActivityAccountingType = 'pie';"
                                                    mdTooltip="Pie Chart" style="font-size: 16px;">pie_chart</i>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="reconBackgroundColor" style="text-align: -webkit-center;height: 210px;width: 250px;">
                                        <canvas *ngIf="showReconCurrencyChart && pieChartActivityAccounting && pieChartActivityAccountingLabels" baseChart [data]="pieChartActivityAccounting"
                                            [labels]="pieChartActivityAccountingLabels" [chartType]="pieChartActivityAccountingType"
                                            [options]="pieChartActivityAccountingOptions" [colors]="pieChartActivityColors" (chartClick)="fetchGrpByData('currency', $event,'Un-Reconciled');reconSrcBlock = true;reconTrgBlock = false;showActiveCls('src');scrollTab();">
                                        </canvas>
                                    </div>
                                </div>
                            </ng-template>
                            <ng-template md-tab-label *ngIf="reconAllData[selectedReconTab].multiCurrency != 'true'">
                                <div class="spinnerBlock"></div>
                                <div [ngClass]="{selectedModule: currentModule == 'Un-Reconciled Transaction By Aging Bucket'}">
                                    <div id="headerTab">
                                        <div class="kpiHeaderFont">Un-Reconciled By Aging Bucket</div>
                                        <div style="position: absolute; right: 0px; top: 12px;">
                                            <span class="headerbutton">
                                                <md-icon (click)="openSideBar('Un-Reconciled Transaction By Aging Bucket');" style="height: 34px !important;visibility:hidden;font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                                            </span>
                                            <span class="material-icons" [mdMenuTriggerFor]="menu" mdTooltip="More">more_vert</span>
                                            <md-menu #menu="mdMenu" xPosition="before">
                                                <button md-menu-item *ngFor="let col of bucketList" [ngClass]="{'nav-active': col == activeItem2}" (click)="getReconAgingBucket(col.id);activeItem2 = col;">
                                                    <span>{{col.bucketName}}</span>
                                                </button>
                                            </md-menu>
                                        </div>
                                    </div>
                                    <div class="agingBackgroundColor" style="text-align: -webkit-center;height: 210px;width: 250px;padding: 6px;cursor: pointer;"
                                        id="reconAgingTest">
                                        <canvas *ngIf="barChartLabels && barChartData && reconAgingChart" baseChart [datasets]="barChartData" [labels]="barChartLabels"
                                            [options]="barChartOptions" [legend]="barChartLegend" [chartType]="barChartType"
                                            [colors]="unReconAgingBucketColors" (chartClick)="fetchGrpByData('aging', $event);reconSrcBlock = true;reconTrgBlock = false;showActiveCls('src');scrollTab();"></canvas>
                                        <div *ngIf="bucketList && bucketList.length<1">There is no "Aging Bucket" defined</div>
                                    </div>
                                </div>
                            </ng-template>
                        </md-tab>
                        <md-tab>
                            <ng-template md-tab-label *ngIf="reconAllData[selectedReconTab].multiCurrency == 'true'">
                                <div class="spinnerBlock"></div>
                                <div [ngClass]="{selectedModule: currentModule == 'Un-Reconciled Transaction By Aging Bucket'}">
                                    <div id="headerTab">
                                        <div class="kpiHeaderFont">Un-Reconciled By Aging Bucket</div>
                                        <div style="position: absolute; right: 0px; top: 12px;">
                                            <span class="headerbutton">
                                                <md-icon (click)="openSideBar('Un-Reconciled Transaction By Aging Bucket');" style="height: 34px !important;visibility:hidden;font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                                            </span>
                                            <span class="material-icons" [mdMenuTriggerFor]="menu" mdTooltip="More">more_vert</span>
                                            <md-menu #menu="mdMenu" xPosition="before">
                                                <button md-menu-item *ngFor="let col of bucketList" [ngClass]="{'nav-active': col == activeItem2}" (click)="getReconAgingBucket(col.id);activeItem2 = col;">
                                                    <span>{{col.bucketName}}</span>
                                                </button>
                                            </md-menu>
                                        </div>
                                    </div>
                                    <div class="agingBackgroundColor" style="text-align: -webkit-center;height: 210px;width: 250px;padding: 6px;cursor: pointer;"
                                        id="reconAgingTest">
                                        <canvas *ngIf="barChartLabels && barChartData && reconAgingChart" baseChart [datasets]="barChartData" [labels]="barChartLabels"
                                            [options]="barChartOptions" [legend]="barChartLegend" [chartType]="barChartType"
                                            [colors]="unReconAgingBucketColors" (chartClick)="fetchGrpByData('aging', $event);reconSrcBlock = true;reconTrgBlock = false;showActiveCls('src');scrollTab();"></canvas>
                                        <div *ngIf="bucketList && bucketList.length<1">There is no "Aging Bucket" defined</div>
                                    </div>
                                </div>
                            </ng-template>
                            <ng-template md-tab-label *ngIf="reconAllData[selectedReconTab].multiCurrency != 'true'">
                                <div style="width: 100%;" (click)="fetchGrpByData('Un-Reconciled');showReconDetilsTab = true;reconSrcBlock = true;reconTrgBlock = false;showActiveCls('src');scrollTab();">
                                    <div id="headerTab">
