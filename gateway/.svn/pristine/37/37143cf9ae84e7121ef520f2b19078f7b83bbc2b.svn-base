                                                <div style="position: relative;">{{row[col.field]}}%</div>
                                                <!--  | number: '.0' -->
                                            </div>
                                        </ng-template>
                                    </p-column>
                                </p-dataTable>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- SIDE BAR -->
            <p-sidebar [(visible)]="visibleSidebar2" position="right" [baseZIndex]="10000">
                <div *ngIf="openModuleSetting == 'Filter Data'">
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-4" style="margin-top: 12px;">Processes :</div>
                        <div class="col-md-8">
                            <perfect-scrollbar style="height: 100px;">
                                <md-nav-list>
                                    <a md-list-item *ngFor="let col of processesList" (click)="selectedProcessFun(col);" [ngClass]="{'nav-active': col == activeItem}">
                                        {{ col.processName }} </a>
                                </md-nav-list>
                            </perfect-scrollbar>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-12" style="margin-top: auto; margin-bottom: auto;">
                            <md-checkbox [(ngModel)]="processFilter.asOfValue">As Of Today</md-checkbox>
                        </div>
                        <div class="col-md-12" *ngIf="processFilter.asOfValue == false" style="display: flex;padding-left: 33px;">
                            <div class="datePickerWidth">
                                <md-form-field>
                                    <input mdInput [mdDatepicker]="stpicker" placeholder="From date" [(ngModel)]="processFilter.startDate" name="startDate" required>
                                    <md-datepicker-toggle mdSuffix [for]="stpicker"></md-datepicker-toggle>
                                    <md-datepicker #stpicker></md-datepicker>
                                </md-form-field>
                            </div>
                            <div class="datePickerWidth">
                                <md-form-field>
                                    <input mdInput [mdDatepicker]="edpicker" placeholder="To date" [min]="processFilter.startDate" [(ngModel)]="processFilter.endDate"
                                        name="endDate">
                                    <md-datepicker-toggle mdSuffix [for]="edpicker"></md-datepicker-toggle>
                                    <md-datepicker #edpicker></md-datepicker>
                                </md-form-field>
                            </div>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-12" style="margin-top: auto; margin-bottom: auto;">
                            <md-checkbox [(ngModel)]="processFilter.closedPeriods">Include Closed Periods</md-checkbox>
                        </div>
                    </div>
                </div>
                <div *ngIf="openModuleSetting == 'Reconciliation Status'">
                    <div class="col-md-12" style="margin-top: 15px;">
                        <h5>{{openModuleSetting}}</h5>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Reconciled Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="reconColorTemp"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Un-Reconciled Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="unReconColorTemp"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Background Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="reconBackgroundTemp"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8">
                            Chart Type :
                        </div>
                        <div class="col-md-4">
                            <md-radio-group [(ngModel)]="temppieChartReconciliationType">
                                <md-radio-button value="pie">Pie</md-radio-button>
                                <md-radio-button value="bar">Bar</md-radio-button>
                            </md-radio-group>
                        </div>
                    </div>
                </div>
                <div *ngIf="openModuleSetting == 'Un-Reconciled Transaction By Aging Bucket'">
                    <div class="col-md-12" style="margin-top: 15px;">
                        <h4>{{openModuleSetting}}</h4>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempagingColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Background Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="agingBucketBackgroundTemp"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8">
                            Chart Type :
                        </div>
                        <div class="col-md-4">
                            <md-radio-group [(ngModel)]="tempbarChartType">
                                <md-radio-button value="pie">Pie</md-radio-button>
                                <md-radio-button value="bar">Bar</md-radio-button>
                                <md-radio-button value="line">Line</md-radio-button>
                            </md-radio-group>
                        </div>
                    </div>
                </div>
                <div *ngIf="openModuleSetting == 'Un-Reconciled Items By Value'">
                    <div class="col-md-12" style="margin-top: 15px;">
                        <h4>{{openModuleSetting}}</h4>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Radius :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempunReconItemsByValueRadius" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Outer Stroke Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByValueOuterStrokeColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Inner Stroke Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByValueInnerStrokeColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByValueTitleColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Sub-Title Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByValueSubTitleColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Font Size :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempunReconItemsByValueTitleFontSize" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Sub-Title Font Size :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempunReconItemsByValueSubtitleFontSize" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                </div>
                <div *ngIf="openModuleSetting == 'Un-Reconciliation Items By Violation'">
                    <div class="col-md-12" style="margin-top: 15px;">
                        <h4>{{openModuleSetting}}</h4>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Radius :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempunReconItemsByViolationRadius" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Outer Stroke Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByViolationOuterStrokeColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Inner Stroke Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByViolationInnerStrokeColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempunReconItemsByViolationTitleColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Font Size :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempunReconItemsByViolationTitleFontSize" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                </div>
                <div *ngIf="openModuleSetting == 'Awaiting Approvals'">
                    <div class="col-md-12" style="margin-top: 15px;">
                        <h4>{{openModuleSetting}}</h4>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Radius :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempreconAwaitingForApprovalRadius" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Outer Stroke Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempreconAwaitingForApprovalOuterStrokeColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Color :
                        </div>
                        <div class="col-md-4">
                            <p-colorPicker [(ngModel)]="tempreconAwaitingForApprovalTitleColor"></p-colorPicker>
                        </div>
                    </div>
                    <div class="row col-md-12" style="margin-left: 0px;padding: 5px;">
                        <div class="col-md-8" style="margin-top: auto; margin-bottom: auto;">
                            Title Font Size :
                        </div>
                        <div class="col-md-4" style="margin-top: -12px;">
                            <md-input-container class="example-full-width ">
                                <input mdInput type="number" [(ngModel)]="tempreconAwaitingForApprovalTitleFontSize" placeholder="">
                            </md-input-container>
                        </div>
                    </div>
                </div>
                <div style="margin-top: 5px;">
                    <button md-raised-button class="pull-right" (click)="cancelReconSetting();visibleSidebar2 = false">Cancel</button>
                    <button md-raised-button class="pull-right" (click)="saveReconSetting();visibleSidebar2 = false">Save</button>
                </div>
            </p-sidebar>
        </div>
    </div>
</div>