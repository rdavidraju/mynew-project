<div class="component-body dashboardV5 versionv6">
    <md-card id="dashboardNavSticky">
        <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-left: 0px;padding: 10px;">
            <div class="col-lg-3 col-md-3 processSelectionTitle">
                <!-- <div>Selected Process : {{selectedProcessName}}</div> -->
                <md-select placeholder="" [(ngModel)]="selectedProcess" name="selectedProcess" style="padding-top: 0px;width: 200px;"><!--   padding-left: 8px;  -->
                    <md-option *ngFor="let col of processesList" [value]="col.id">
                        {{ col.processName }}
                    </md-option>
                </md-select>
            </div>
            <!-- <div class="col-lg-2 col-md-2">
                <md-checkbox [(ngModel)]="processFilter.asOfValue">As Of Today</md-checkbox>
            </div>
            <div class="col-lg-4 col-md-4" style="display: flex;margin-top: -15px;margin-left: -110px;" *ngIf="processFilter.asOfValue == false">
                <div class="datePickerWidth">
                    <md-form-field> <input mdInput [mdDatepicker]="stpicker" placeholder="Start date" [ngClass]="{'color-black' : isViewOnly}"
                            [disabled]="isViewOnly" [(ngModel)]="processFilter.startDate" name="startDate" required>
                        <md-datepicker-toggle mdSuffix [for]="stpicker" *ngIf="!isViewOnly"></md-datepicker-toggle>
                        <md-datepicker #stpicker></md-datepicker>
                    </md-form-field>
                </div>
                <div class="datePickerWidth">
                    <md-form-field> <input mdInput [mdDatepicker]="edpicker" placeholder="End date" [min]="processFilter.startDate" [ngClass]="{'color-black' : isViewOnly}"
                            [disabled]="isViewOnly" [(ngModel)]="processFilter.endDate" name="endDate">
                        <md-datepicker-toggle mdSuffix [for]="edpicker" *ngIf="!isViewOnly"></md-datepicker-toggle>
                        <md-datepicker #edpicker></md-datepicker>
                    </md-form-field>
                </div>
            </div>
            <div class="col-lg-2 col-md-2">
                <md-checkbox [(ngModel)]="processFilter.closedPeriods">Include Closed Periods</md-checkbox>
            </div> -->
            <div class="col-lg-8 col-md-8 mainNav" style="margin-top: -10px;margin-bottom: -14px;">
                <!-- <md-button-toggle-group #group="mdButtonToggleGroup">
                    <md-button-toggle value="center">
                        Reconciliation
                    </md-button-toggle>
                    <md-button-toggle value="center">
                        Accounting
                    </md-button-toggle>
                </md-button-toggle-group> -->
                <md-tab-group style="float:right;" (focusChange)="changeModuleType($event);" [selectedIndex]="selectedModuleType">
                    <md-tab label="Reconciliation"></md-tab>
                    <md-tab label="Accounting"></md-tab>
                </md-tab-group>
            </div>
            <div class="col-lg-1 col-md-1" style="position: absolute;right: 0;top: 5px;">
                <button md-icon-button class="pull-right" aria-hidden="true" mdTooltip="Filter Data" (click)="openSideBar('Filter Data');">
				    <md-icon>filter_list</md-icon>
				</button>
            </div>
        </div>
    </md-card>
    <md-card id="reconciliationDiv">
        <md-card-header>
            <md-card-title>Reconciliation</md-card-title>
        </md-card-header>
        <!-- <canvas *ngIf="testshow && testLabels" baseChart [datasets]="testDataSets"
                    [chartType]="line"        [labels]="testLabels"  [options]="testOptions">
                        </canvas> -->

        <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12 reconGrids" style="margin-left: 0px;margin-top: 10px;padding: 0px;">
            <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12" [ngClass]="{selectedModule: currentModule == 'Reconciliation Status'}"
                (click)="showData('Reconciliation Status')" style="padding-left: 0px;">
                <md-card>
                    <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                        <div style="font-size: 14px;">Reconciliation Status</div>
                        <div style="position: absolute; right: 0px; top: 1px;height: 34px;">
                            <button class="headerbutton" md-icon-button style="height: 34px !important;visibility:hidden;" (click)="openSideBar('Reconciliation Status');">
                            <md-icon style="font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                        </button>
                        </div>
                    </div>
                    <div class="reconBackgroundColor" style="text-align: -webkit-center;height:210px;" id="reconStatusTest">
                        <canvas *ngIf="showRecon && pieChartReconciliation && pieChartReconciliationLabels" baseChart [data]="pieChartReconciliation"
                            [labels]="pieChartReconciliationLabels" [chartType]="pieChartReconciliationType" [options]="pieChartReconciliationOptions"
                            [colors]="pieChartReconciliationColors" (chartClick)="fetchReconciliationStatus($event)">
                        </canvas>
                    </div>
                </md-card>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12 " [ngClass]="{selectedModule: currentModule == 'Un-Reconcilied Transaction By Aging Bucket'}"
                (click)="showData('Un-Reconcilied Transaction By Aging Bucket')">
                <md-card>
                    <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                        <div style="font-size: 14px;">Un-Reconcilied By Aging Bucket</div>
                        <div style="position: absolute; right: 0px; top: 1px;height: 34px;">
                            <button class="headerbutton" md-icon-button style="height: 34px !important;visibility:hidden;" (click)="openSideBar('Un-Reconcilied Transaction By Aging Bucket');">
                            <md-icon style="font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                        </button>
                        </div>
                    </div>
                    <div class="agingBackgroundColor" style="text-align: -webkit-center;height: 210px;padding: 6px;cursor: pointer;" id="reconAgingTest">
                        <canvas *ngIf="barChartLabels && barChartData" baseChart [datasets]="barChartData" [labels]="barChartLabels" [options]="barChartOptions"
                            [legend]="barChartLegend" [chartType]="barChartType" [colors]="unReconAgingBucketColors"
                            (chartClick)="fetchUnReconciliationAging($event)"></canvas>
                    </div>
                </md-card>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12" [ngClass]="{selectedModule: currentModule == 'Top 10 Un-Reconcilied Items By Value'}"
                (click)="showData('Top 10 Un-Reconcilied Items By Value')">
                <md-card>
                    <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                        <div style="font-size: 14px;">Un-Reconcilied Items By Value</div>
                        <div style="position: absolute; right: 0px; top: 1px;height: 34px;">
                            <button class="headerbutton" md-icon-button style="height: 34px !important;visibility:hidden;" (click)="openSideBar('Un-Reconcilied Items By Value');">
                            <md-icon style="font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                        </button>
                        </div>
                    </div>
                    <div class="reconBackgroundColor" style="min-height: 210px;">
                        <circle-progress [percent]="topTenUnReconValue" [radius]="unReconItemsByValueRadius" [space]="-6" [outerStrokeWidth]="6"
                            [innerStrokeWidth]="6" [outerStrokeColor]="unReconItemsByValueOuterStrokeColor" [innerStrokeColor]="unReconItemsByValueInnerStrokeColor"
                            [animation]="true" [animationDuration]="300" [showUnits]="false" [showtitle]="true" [title]="topTenUnReconValueTitle"
                            [titleFontSize]="unReconItemsByValueTitleFontSize" [unitsFontSize]="15" [showSubtitle]="true" [subtitleFontSize]="unReconItemsByValueSubtitleFontSize"
                            [subtitle]="topTenUnReconValueSub" [titleColor]="unReconItemsByValueTitleColor" [subtitleColor]="unReconItemsByValueSubTitleColor"
                            [unitsColor]="unReconItemsByValueUnitsColor"></circle-progress>
                    </div>
                </md-card>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12" [ngClass]="{selectedModule: currentModule == 'Un-Reconciliation Items By Violation'}"
                (click)="showData('Un-Reconciliation Items By Violation')">
                <!--  (mouseenter)="showData('reconciliation')" [ngClass]="{activeModule: currentModule == 'reconciliation'}" -->
                <md-card>
                    <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                        <div style="font-size: 14px;padding-left: 10px;">Un-Reconcilied Items By Violation</div>
                        <div style="position: absolute; right: 0px; top: 1px;height: 34px;">
                            <button class="headerbutton" md-icon-button style="height: 34px !important;visibility:hidden;" (click)="openSideBar('Un-Reconciliation Items By Violation');">
                            <md-icon style="font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                        </button>
                        </div>
                    </div>
                    <div class="reconBackgroundColor" style="min-height: 210px;">
                        <circle-progress [percent]="topTenUnReconViolation" [radius]="unReconItemsByViolationRadius" [space]="-6" [outerStrokeWidth]="6"
                            [innerStrokeWidth]="6" [outerStrokeColor]="unReconItemsByViolationOuterStrokeColor" [innerStrokeColor]="unReconItemsByViolationInnerStrokeColor"
                            [animation]="true" [animationDuration]="300" [showUnits]="false" [showtitle]="true" [title]="topTenUnReconViolationTitle"
                            [titleFontSize]="unReconItemsByViolationTitleFontSize" [unitsFontSize]="15" [subtitle]="'Transaction'" [subtitleFontSize]="18"
                            [titleColor]="unReconItemsByViolationTitleColor" [showSubtitle]="true"></circle-progress>
                        <!-- [subtitleColor]="unReconItemsByViolationSubTitleColor" -->
                    </div>
                </md-card>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12" [ngClass]="{selectedModule: currentModule == 'Awaiting Approvals'}" (click)="showData('Awaiting Approvals')"
                style="padding-right: 0px;">
                <!--  (mouseenter)="showData('reconciliation')" [ngClass]="{activeModule: currentModule == 'reconciliation'}" -->
                <md-card>
                    <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                        <div style="font-size: 14px;">Awaiting For Approvals</div>
                        <div style="position: absolute; right: 0px; top: 1px;height: 34px;">
                            <button class="headerbutton" md-icon-button style="height: 34px !important;visibility:hidden;" (click)="openSideBar('Awaiting Approvals');">
                            <md-icon style="font-size: 16px !important;margin-top: -12px !important;">settings</md-icon>
                        </button>
                        </div>
                    </div>
                    <div class="reconBackgroundColor" style="min-height: 210px;">
                        <circle-progress [percent]="awaitingApproval" [radius]="reconAwaitingForApprovalRadius" [space]="-6" [outerStrokeWidth]="6"
                            [innerStrokeWidth]="6" [outerStrokeColor]="reconAwaitingForApprovalOuterStrokeColor" [innerStrokeColor]="reconAwaitingForApprovalInnerStrokeColor"
                            [animation]="true" [animationDuration]="300" [showUnits]="false" [showtitle]="true" [title]="awaitingApprovalCount"
                            [titleFontSize]="reconAwaitingForApprovalTitleFontSize" [titleColor]="reconAwaitingForApprovalTitleColor"
                            [subtitle]="'Approvals'" [showSubtitle]="true" [subtitleFontSize]="18"></circle-progress>
                    </div>
                </md-card>
            </div>
        </div>
        <div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-left: 0px;margin-top: 10px;padding: 0px;">
            <md-card class="dashboardNavTab">
                <div id="headerTab" style="line-height: 34px; border-bottom: 1px solid rgba(211, 211, 211, 0.64);  background: rgba(211, 211, 211, 0.2);text-align: center;">
                    <div style="font-size: 18px;">{{currentModule}} <span *ngIf="currentModule == 'Un-Reconcilied Transaction By Aging Bucket' && showAgingFilterData">{{selectedRegion}} <span *ngIf="unReconAgingBucketDataSelected.data.length>9" style="font-size: 12px;">(Top 10 Records)</span></span></div>
                    <div *ngIf="currentModule == 'Reconciliation Status'" class="selectLovFont" style="position: absolute; right: 0px; top: 1px;height: 34px;display:flex;">
                        <div style="font-size: 13px;">View By: </div>
                        <div *ngIf="currentModule == 'Reconciliation Status'">
                            <md-select *ngIf="selectedTab == 1" placeholder="" style="padding-top: 0px;padding-left: 5px;padding-right: 5px;" [(ngModel)]="selectedViewRS">
                                <md-option value="rules">Rules</md-option>
                            </md-select>
                            <md-select *ngIf="selectedTab == 0" placeholder="" style="padding-top: 0px;padding-left: 5px;padding-right: 5px;" [(ngModel)]="selectedViewRSUnRecon">
                                <md-option value="currency">Currency</md-option>
                            </md-select>
                        </div>
                    </div>
                    <div *ngIf="currentModule == 'Un-Reconcilied Transaction By Aging Bucket'" class="selectLovFont" style="position: absolute; right: 0px; top: 1px;height: 34px;display:flex;">
                        <div style="font-size: 13px;">Type: </div>
                        <div *ngIf="currentModule == 'Un-Reconcilied Transaction By Aging Bucket'">
                            <md-select placeholder="" style="padding-top: 0px;padding-left: 5px;padding-right: 5px;" [(ngModel)]="selectedViewRSAging">
                                <md-option value="Aging Bucket 4">Aging Bucket 4</md-option>
                            </md-select>
                        </div>
                    </div>
                    <div *ngIf="currentModule == 'Top 10 Un-Reconcilied Items By Value'" class="selectLovFont" style="position: absolute; right: 0px; top: 1px;height: 34px;display:flex;">
                        <div style="font-size: 13px;">Type: </div>
                        <div *ngIf="currentModule == 'Top 10 Un-Reconcilied Items By Value'">
                            <md-select placeholder="" style="padding-top: 0px;padding-left: 5px;padding-right: 18px;width: 107%;" [(ngModel)]="selectedUnReconItemsValue">
                                <md-option value="Top 10 by amount">Top 10 by amount</md-option>
                            </md-select>
                        </div>
                    </div>
                    <div *ngIf="currentModule == 'Un-Reconciliation Items By Violation'" class="selectLovFont" style="position: absolute; right: 0px; top: 1px;height: 34px;display:flex;">
                        <div style="font-size: 13px;">Type: </div>
                        <div>
                            <md-select placeholder="" style="padding-top: 0px;padding-left: 5px;padding-right: 18px;width: 107%;" [(ngModel)]="selectedUnReconItemsViolationValue">
                                <md-option value=">= 415">>= 415</md-option>
                                <md-option value="0 - 10 Days">0 - 10 Days</md-option>
                                <md-option value="11 - 30 Days">11 - 30 Days</md-option>
                            </md-select>
                        </div>
                    </div>
                </div>
                <md-tab-group [@flyInOut] *ngIf="currentModule == 'Reconciliation Status'" style="padding: 5px;" [selectedIndex]="selectedTab"
                    (focusChange)="changeTab($event);">
                    <md-tab label="Un-Reconcilied">
                        <div class="table-responsive" style="padding-top: 5px;">
                            <p-dataTable [value]="reconciliationStatusData.unReconcilied" [responsive]="true">
                                <p-headerColumnGroup>
                                    <p-row>
                                        <p-column header="Currency Code"></p-column>
                                        <p-column header="Ledger Name"></p-column>
                                        <p-column header="Count" [style]="{'text-align':'center'}"></p-column>
                                        <p-column header="Amount" [style]="{'text-align':'right'}"></p-column>
                                        <p-column header="Percentage" [style]="{'text-align':'center'}"></p-column>
                                    </p-row>
                                </p-headerColumnGroup>
                                <p-column field="Currency Code"></p-column>
                                <p-column field="Ledger Name"></p-column>
                                <p-column field="Count" [styleClass]="'countRight'"></p-column>
                                <p-column field="Amount" [style]="{'text-align':'right'}"></p-column>
                                <!-- <p-column field="Percentage" [style]="{'text-align':'right'}"></p-column> -->
                                <p-column field="Percentage" header="Percentage" [style]="{'text-align':'center'}">
                                    <ng-template let-col let-row="rowData" pTemplate="body">
                                        <!-- <div style="display:flex;margin-top: -11px;">
                                            <md-progress-bar mode="determinate" value="{{row[col.field]}}"></md-progress-bar>
                                            <div style="position: absolute;right: 5px;">{{row[col.field]}}%</div>
                                        </div> -->
                                        <div style="text-align:center;">
                                            <div style="height: 15px;margin-top: -13px;" mdTooltip="{{row[col.field]}}%">
                                                <circle-progress [percent]="row[col.field]" [radius]="8" [space]="-2" [outerStrokeWidth]="2" [innerStrokeWidth]="2" [outerStrokeColor]="'#2295f3'"
                                                    [innerStrokeColor]="'#bbdffb'" [unitsFontSize]="0" [titleFontSize]="0" [animation]="true"
                                                    [animationDuration]="300" [showUnits]="false" [showtitle]="false" [showSubtitle]="false">
                                                </circle-progress>
                                            </div>
                                            <!-- <div style="margin-top: auto;margin-bottom: auto;">{{row[col.field]}}%</div> -->
                                        </div>
                                    </ng-template>
                                </p-column>
                            </p-dataTable>
                        </div>
                    </md-tab>
                    <md-tab label="Reconcilied">
                        <div class="table-responsive" style="padding-top: 5px;">
                            <p-dataTable [value]="reconciliationStatusData.reconcilied" [responsive]="true">
                                <p-headerColumnGroup>
                                    <p-row>
                                        <p-column header="Rule Name"></p-column>
                                        <p-column header="Ledger Name"></p-column>
                                        <p-column header="Count" [style]="{'text-align':'center'}"></p-column>
                                        <p-column header="Amount" [style]="{'text-align':'right'}"></p-column>
                                        <p-column header="Percentage" [style]="{'text-align':'right'}"></p-column>
                                    </p-row>
                                </p-headerColumnGroup>
                                <p-column field="Rule Name"></p-column>
                                <p-column field="Ledger Name"></p-column>
                                <p-column field="Count" [styleClass]="'countRight'"></p-column>
                                <p-column field="Amount" [style]="{'text-align':'right'}"></p-column>
                                <!-- <p-column field="Percentage" [style]="{'text-align':'right'}"></p-column> -->
                                <p-column field="Percentage" header="Percentage" [style]="{'text-align':'right'}">
                                    <ng-template let-col let-row="rowData" pTemplate="body">
                                        <div style="display:flex;float: right;">
                                            <div style="height: 15px;margin-top: -4px;">
                                                <circle-progress [percent]="row[col.field]" [radius]="8" [space]="-2" [outerStrokeWidth]="2" [innerStrokeWidth]="2" [outerStrokeColor]="'#2295f3'"
                                                    [innerStrokeColor]="'#bbdffb'" [unitsFontSize]="0" [titleFontSize]="0" [animation]="true"
                                                    [animationDuration]="300" [showUnits]="false" [showtitle]="false" [showSubtitle]="false">
                                                </circle-progress>
                                            </div>
                                            <div style="margin-top: auto;margin-bottom: auto;">{{row[col.field]}}%</div>
                                        </div>
                                    </ng-template>
                                </p-column>

                                <p-footerColumnGroup>
                                    <p-row>
                                        <p-column footer="Grand Total" colspan="2"></p-column>
                                        <p-column footer="4,182" [style]="{'text-align':'center'}"></p-column>
                                        <p-column footer="2,583,359,230.78" [style]="{'text-align':'right'}"></p-column>
                                        <p-column footer="100%" [style]="{'text-align':'right'}"></p-column>
                                    </p-row>
                                </p-footerColumnGroup>
                            </p-dataTable>
                        </div>
                    </md-tab>
                </md-tab-group>
                <div [@flyInOut] *ngIf="currentModule == 'Un-Reconcilied Transaction By Aging Bucket' && !showAgingFilterData">
                    <div class="table-responsive" style="padding: 5px;">
                        <p-dataTable [value]="unReconAgingBucketData" [responsive]="true">
                            <p-headerColumnGroup>
                                <p-row>
                                    <p-column header="Age"></p-column>
                                    <p-column header="Count" [style]="{'text-align':'center'}"></p-column>
                                    <p-column header="Amount" [style]="{'text-align':'right'}"></p-column>
                                    <p-column header="Percentage" [style]="{'text-align':'right'}"></p-column>
                                </p-row>
                            </p-headerColumnGroup>
                            <p-column field="Age"></p-column>
                            <p-column field="Count" [styleClass]="'countRight'"></p-column>
                            <p-column field="Amount" [style]="{'text-align':'right'}"></p-column>
                            <!-- <p-column field="Percentage" [style]="{'text-align':'right'}"></p-column> -->
                            <p-column field="Percentage" header="Percentage" [style]="{'text-align':'right'}">
                                <ng-template let-col let-row="rowData" pTemplate="body">
                                    {{row[col.field]}}%
                                </ng-template>
                            </p-column>

                            <p-footerColumnGroup>
                                <p-row>
                                    <p-column footer="Grand Total"></p-column>
                                    <p-column footer="701" [style]="{'text-align':'center'}"></p-column>
                                    <p-column footer="1,21,95,48,504.87" [style]="{'text-align':'right'}"></p-column>
                                    <p-column footer="100%" [style]="{'text-align':'right'}"></p-column>
                                </p-row>
                            </p-footerColumnGroup>
                        </p-dataTable>
                    </div>
                </div>
                <div [@flyInOut] *ngIf="currentModule == 'Un-Reconcilied Transaction By Aging Bucket' && showAgingFilterData">
                    <div class="table-responsive" style="padding: 5px;">
                        <p-dataTable [value]="unReconAgingBucketDataSelected.data" [responsive]="true">
                            <p-headerColumnGroup>
                                <p-row>
                                    <p-column header="Provider Name"></p-column>
                                    <p-column header="Ledger Name"></p-column>
                                    <p-column header="Currency"></p-column>
                                    <p-column header="Amount" [style]="{'text-align':'right'}"></p-column>
                                    <p-column header="Percentage" [style]="{'text-align':'right'}"></p-column>
                                </p-row>
                            </p-headerColumnGroup>
                            <p-column field="Provider Name"></p-column>
                            <p-column field="Ledger Name"></p-column>
                            <p-column field="Currency"></p-column>
                            <p-column field="Amount" [style]="{'text-align':'right'}"></p-column>
                            <!-- <p-column field="Percentage" [style]="{'text-align':'right'}"></p-column> -->
                            <p-column field="Percentage" header="Percentage" [style]="{'text-align':'right'}">
                                <ng-template let-col let-row="rowData" pTemplate="body">
                                    {{row[col.field]}}%
                                </ng-template>
                            </p-column>

                            <p-footerColumnGroup>
                                <p-row>
                                    <p-column footer="Grand Total" colspan="3"></p-column>
                                    <p-column [footer]="unReconAgingBucketDataSelected.totalAmount" [style]="{'text-align':'right'}"></p-column>
                                    <p-column [footer]="unReconAgingBucketDataSelected.totalPercentage" [style]="{'text-align':'right'}"></p-column>
                                </p-row>
                            </p-footerColumnGroup>
                        </p-dataTable>
                    </div>
                </div>
                <div [@flyInOut] *ngIf="currentModule == 'Top 10 Un-Reconcilied Items By Value'">
                    <div class="table-responsive" style="padding: 5px;">
                        <p-dataTable [value]="topTenUnReconItemData" [responsive]="true">
                            <p-headerColumnGroup>
                                <p-row>
                                    <p-column header="Provider Name"></p-column>
                                    <p-column header="Ledger Name"></p-column>
                                    <p-column header="Sum Of Amount" [style]="{'text-align':'right'}"></p-column>
                                    <p-column header="Percentage" [style]="{'text-align':'right'}"></p-column>
                                </p-row>
                            </p-headerColumnGroup>
                            <p-column field="Provider Name"></p-column>
                            <p-column field="Ledger Name"></p-column>
                            <p-column field="Sum Of Amount" [style]="{'text-align':'right'}"></p-column>
                            <p-column field="Percentage" header="Percentage" [style]="{'text-align':'right'}">
                                <ng-template let-col let-row="rowData" pTemplate="body">
