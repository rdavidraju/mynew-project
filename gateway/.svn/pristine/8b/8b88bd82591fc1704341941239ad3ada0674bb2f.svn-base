//******* New Sidebar Styles *******//
.sidebarToggle {
    .ng-sidebar {
        margin-top: 48px;
        z-index: 1001 !important;
        width: 65px;
        white-space: nowrap;
        overflow: hidden !important;
        position: fixed !important;
        transition: 0.5s !important;
        box-shadow: 1px 1px 3px 0px #ececec;

        .ui-state-active {
            .accordian-minus {
                visibility: visible !important;
            }

            .accordian-plus {
                visibility: hidden !important;
            }
        }

        .ui-corner-all {
            border-radius: 0 !important;
        }

        .accordianSidebar {
            perfect-scrollbar {
                background: none !important;
            }

            height: 100%;

            // background: url(../images/01.jpg);
            background-size: cover;
            position: relative;
            box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
            user-select: none;
            border-top: 1px solid lightgray;

            .sidebar-profile {
                -webkit-box-shadow: 0px 2px 5px 2px rgba(0, 0, 0, 0.15);
                -moz-box-shadow: 0px 2px 5px 2px rgba(0, 0, 0, 0.15);
                box-shadow: 0px 2px 5px 2px rgba(0, 0, 0, 0.15);
                height: 80px;
                position: relative;
                transition: all 0.4s ease;

                .display-picture {
                    border-radius: 100%;
                    width: 53px;
                    height: 53px;
                    position: absolute;
                    right: 7px;
                    transition-delay: 0.5s;
                    background-repeat: no-repeat;

                    // background-size: 120px;
                    // background-position: -25px;
                    top: 13px;

                    &:before {
                        content: attr(data-content);
                        position: absolute;
                        top: 50%;
                        left: 50%;
                        transform: translate(-50%, -50%);
                        font-weight: bold;
                        font-size: 30px;
                        color: #2196F3;
                    }
                }

                .display-name {
                    position: relative;
                    top: 15px;
                    left: 0px;
                    font-size: 14px;

                    // font-family: Poppins,sans-serif;
                    transition: all 0.5s ease;
                }

                p {
                    position: relative;
                    top: 8px;
                    padding: 4px 0;
                    font-size: 12px;
                    left: 0px;
                    transition: all 0.5s ease;
                }
            }

            .mat-expansion-panel-header-title {
                font-weight: normal;
            }

            .mat-expansion-panel,
            .mat-expansion-panel:not([class*=mat-elevation-z]) {
                border-right: 0px solid #f5f7fa !important;
                box-shadow: unset !important;
                background: transparent !important;

                .mat-expansion-panel-header {
                    padding: 0 14px;
                }

                .mat-expansion-panel-body {
                    padding: 0px 0px 0px;
                }

                .mat-expansion-indicator::after {
                    color: white;
                }

                .mat-expansion-indicator::after {
                    border-style: solid;
                    border-width: 0 1px 1px 0;
                    content: '';
                    display: inline-block;
                    padding: 2px;
                    transform: rotate(45deg);
                    vertical-align: middle;
                }

                // .inner-accordion {
                // 	.mat-expansion-panel-header-title {
                // 		padding-left: 15px;
                // 	}
                // }
           
            }
        }

        .mat-expansion-panel-header {
            min-height: 40px;
            max-height: 40px;
        }

        .accordianSidebar {
            .mat-expansion-panel-header.mat-expanded span {
                font-weight: bold;
            }
        }
    }
}

.sidebarToggle .ng-sidebar-iconWidth {
    width: 70px;
    transition: 0.5s;

    .accordianSidebar {
        .sidebar-profile {
            transition: all 0.4s ease;
            height: 80px;

            .display-picture {
                transition: all 0s ease;
                transition-delay: 0.5s;
            }

            .display-name {
                left: -200px;
                transition: all 0.5s ease;
            }

            p {
                left: -200px;
                transition: all 0.5s ease;
            }
        }
    }

    .sidebar-pin {
        display: none;
    }
}

.sidebarToggle .ng-sidebar-fullWidth {
    width: 240px;
    transition: 0.5s;

    .accordianSidebar {
        .sidebar-profile {
            height: 80px;
            transition: all 0.4s ease;

            .display-picture {
                transition-delay: 0.4s;
            }

            .display-name {
                transition: all 0.5s ease;
            }

            p {
                transition: all 0.5s ease;
            }
        }
    }
}

.sidebarToggle .ng-sidebar .ui-accordion {
    position: relative;

    .ui-accordion-header {
        border-bottom: 1px solid rgba(211, 211, 211, 0.14) !important;
        padding: 5px;
        transition: all .1s ease;
        background: transparent;

        &:not(.ui-state-active):not(.ui-state-disabled):hover {
            background: transparent;
        }
    }

    .ui-widget-content {
        background: transparent;
    }
}

.nestedAccordian .ui-state-active, .nestedAccordian .ui-accordion .ui-accordion-header:hover {
    border-left: none !important;
}

.sidebarToggle .ng-sidebar .ui-widget-content {
    padding: 0;
}

.sidebarToggle .ng-sidebar .ui-accordion .ui-accordion-header a {
    text-decoration: none;
    font-weight: normal;
}

