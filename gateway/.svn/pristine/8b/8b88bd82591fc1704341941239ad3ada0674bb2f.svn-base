.right-sidebar {
    .ng-sidebar {
		width: 50px;
		background: #ffffff;
		margin-top: 10%;
		margin-bottom: 10%;
		border-right-color: transparent;
		border-top-left-radius: 4px;
		border-bottom-left-radius: 4px;
		right: -52px;
		white-space: nowrap;
		overflow: unset !important;
		position: fixed !important;
		box-shadow: 0px 0px 5px 0px #ff6228;
		transition: 0.5s !important;
		ul .outer-li {
			position: relative;
			&:hover {
				.inner-ul {
					left: -13rem;
					padding: 3px 5px;
					margin-bottom: 5px;
				}
			}
			.inner-ul {
				position: absolute;
				left: 12rem;
				top: 0rem;
				transition:0.5s;
				background: rgb(209, 241, 255);
				border-radius: 2px;
				border: 1px solid rgb(209, 241, 255);
				width: 200px;
			}
		}
	}	
	.right-toggle-icon {
		background: #ff6b34;
		position: fixed;
		top: 16%;
		right: 0px;
		padding: 2px;
		border-top-left-radius: 2px;
		border: 1px solid #ff6b34;
		border-bottom-left-radius: 2px;
		transition: 0.5s !important;
		z-index: 9999;
		font-size: 18px;
	}
	.right-toggle-icon.toggled {
		// right: 50px;
		transition: 0.5s !important;
	}
}

.right-sidebar {
	ng-sidebar[ng-reflect-opened = true] {
		.ng-sidebar {
			right : 0;
		}
	}
    
}
