.sidebarToggle .ng-sidebar-iconWidth {
    .sidenav-list-items li {
        padding: 10px 15px;
        transition: padding 0.5s ease-in-out;
    }

    .multi-nested .accordianHeader {
        padding-left: 0px !important;
        transition: padding 0.5s ease-in-out;
    }

    .nest-nested li.start.ripple {
        padding-left: 16px;
        transition: padding 0.5s ease-in-out;
    }

    .mat-expansion-panel,
    .mat-expansion-panel:not([class*=mat-elevation-z]) {
        .inner-accordion {
            .mat-expansion-panel-header-title {
                padding-left: 4px;
                transition: padding 0.5s ease-in-out;
            }
        }
    }

    .mat-expansion-indicator {
        display: none;
    }
}

.sidebarToggle .ng-sidebar-fullWidth {
    .sidenav-list-items li {
        padding: 10px 15px;
        transition: padding 0.5s ease-in-out;
    }

    .multi-nested .accordianHeader {
        padding-left: 13px !important;
        transition: padding 0.5s ease-in-out;
    }

    .nest-nested li.start.ripple {
        padding-left: 33px;
        transition: padding 0.5s ease-in-out;
    }

    .mat-expansion-indicator {
        display: block;
    }

    .mat-expansion-panel,
    .mat-expansion-panel:not([class*=mat-elevation-z]) {
        .inner-accordion {
            .mat-expansion-panel-header-title {
                padding-left: 4px;
                transition: padding 0.5s ease-in-out;
            }
        }
    }
}

.sidebarToggle .ng-sidebar-iconWidth {
    .ui-state-active {
        .accordian-plus, .accordian-minus {
            visibility: hidden !important;
        }
    }

    .accordian-plus, .accordian-minus {
        visibility: hidden !important;
    }
}

.accordian-plus, .accordian-minus {
    float: right;
    margin-top: 10px;
}

.accordian-minus {
    visibility: hidden;
    position: relative;
    left: 8px;
}

.accordian-plus {
    visibility: visible;
}

.sidebarToggle:hover .accordian-minus,
.sidebarToggle:hover .accordian-plus {
    transition: visibility 0.5s linear;
}

.sidebar-static {
    .accordian-minus,
    .accordian-plus {
        transition: unset !important;
    }
}

