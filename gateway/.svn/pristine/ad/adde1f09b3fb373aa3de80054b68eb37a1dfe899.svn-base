<h1 md-dialog-title></h1>

<div md-dialog-content>
   <p *ngIf="data.message">{{data.message}}</p>

</div>

<div md-dialog-actions align="center">
  <div>
    <button md-button [disabled]="data.copyItemIndex == -1" [md-dialog-close]="data.ok" tabindex="2"*ngIf="data.message">Ok</button>
  </div>
</div>