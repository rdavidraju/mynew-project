                            <md-option value="ALL" *ngIf="ledgersList && ledgersList.length>1" (click)="fetchBatchDetailedList()">All</md-option>
                            <md-option *ngFor="let col of ledgersList | searchFilter:jeLedgerSearch" [value]="col" (click)="fetchBatchDetailedList()">
                                {{ col }}
                            </md-option>
                        </md-select>
                    </div>

                    <div class="col-md-2 col-sm-12 col-xs-12">
                        <md-select placeholder="Period" [(ngModel)]="journalsHeaderDataView.jePeriod" [ngClass]="{'color-black' : isViewOnly}"
                            [disabled]="journalsHeaderDataView.jePeriod == 'none'"
                            [materialFilter]="jePeriod" #jePeriod name="jePeriod" [materialFilterModel]="jePeriodSearchModel" [materialFilterInput]="jePeriodSearchInput">
