                            <md-option value=0 (click)="noBatcheTemplate = false;fetchLedgerAndPeriodList('JournalBatches');" *ngIf="journalsHeaderData && journalsHeaderData.length>1">All</md-option>
                            <md-option *ngFor="let col of journalsHeaderData | searchFilter:jeTempIdSearch" [value]="col.id" (click)="noBatcheTemplate = false;fetchLedgerAndPeriodList(col.templateName,col.id,col);">
                                {{ col.templateName }}
                            </md-option>
                        </md-select>
                    </div>
                    <div class="col-md-2 col-sm-12 col-xs-12">
                        <md-select placeholder="Ledger"[(ngModel)]="journalsHeaderDataView.jeLedger" [ngClass]="{'color-black' : isViewOnly}"
