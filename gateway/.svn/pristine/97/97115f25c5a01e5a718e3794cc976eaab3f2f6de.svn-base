@font-face {
  font-family: 'recon';
  src:  url('fonts/recon.eot?tjavgx');
  src:  url('fonts/recon.eot?tjavgx#iefix') format('embedded-opentype'),
    url('fonts/recon.ttf?tjavgx') format('truetype'),
    url('fonts/recon.woff?tjavgx') format('woff'),
    url('fonts/recon.svg?tjavgx#recon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="rec-font-"], [class*=" rec-font-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'recon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

