<h1 md-dialog-title>Confirm!</h1>
<div class="cnfrm" md-dialog-content>
    <p>{{data.message}}</p>
</div>
<div md-dialog-actions align="center">
      <button md-raised-button [md-dialog-close]="data.ok" tabindex="2">{{data.okLabel}}</button>
      <button md-raised-button [md-dialog-close]="data.cancel" tabindex="-1">{{data.cancelLabel}}</button>
</div>