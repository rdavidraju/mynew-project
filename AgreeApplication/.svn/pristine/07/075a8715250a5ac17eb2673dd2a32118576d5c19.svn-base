            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        TenantConfigModules tenantConfigModules = (TenantConfigModules) o;
        if (tenantConfigModules.id == null || id == null) {
            return false;
        }
        return Objects.equals(id, tenantConfigModules.id);
