<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity DataViews.
    -->
    <changeSet id="20170628142058-1t" author="jhipster">
        <createTable tableName="t_data_views">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="tenant_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="data_view_disp_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="data_view_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="data_view_object" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="enabled_flag" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="created_by" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="last_updated_by" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="creation_date" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="last_updated_date" type="timestamp">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="t_data_views" columnName="creation_date" columnDataType="datetime"/>
        <dropDefaultValue tableName="t_data_views" columnName="last_updated_date" columnDataType="datetime"/>
        
    </changeSet>
    
    <changeSet id="addingcolumnsStartAndEndDates" author="ravali">
		<addColumn tableName="t_data_views">
			<column name="start_date" type="date" />
		</addColumn>
		<addColumn tableName="t_data_views">
			<column name="end_date" type="date" />
		</addColumn>
	</changeSet>
	
	
	 <changeSet id="addingDescriptionColumn" author="ravali">
		<addColumn tableName="t_data_views">
			<column name="description" type="varchar(255)" />
		</addColumn>
		
	</changeSet>
	
	<changeSet author="ravali" id="colForDisplayDv">
	<addColumn tableName="t_data_views">
		<column name="id_for_display" type="varchar(255)" beforeColumn="id" />
	</addColumn>
</changeSet>

<!-- <changeSet author="swetha" id="addRecordCount">
	<addColumn tableName="t_data_views">
		<column name="record_count" type="integer"/>
	</addColumn>
</changeSet> -->

<changeSet author="swetha" id="modifyformulaDtype">
	<modifyDataType columnName="formula" newDataType="TEXT"
	tableName="t_data_views_columns"/>
	
	<modifyDataType columnName="formula_alias" newDataType="TEXT"
	tableName="t_data_views_columns"/>
</changeSet>

<changeSet author="ravali" id="modifyDataTypetoTimeStamp_DataViews">
		<modifyDataType columnName="start_date" newDataType="timestamp null"
			tableName="t_data_views" />


		<modifyDataType columnName="end_date" newDataType="timestamp null"
			tableName="t_data_views" />

	</changeSet>
	
	<changeSet author="swetha" id="addMaxFileDate">
	<addColumn tableName="t_data_views">
		<column name="last_file_date" type="timestamp"/>
	</addColumn>
	<addColumn tableName="t_data_views">
		<column name="last_trans_date" type="timestamp"/>
	</addColumn>
	
	 <dropDefaultValue tableName="t_data_views" columnName="last_file_date" columnDataType="datetime"/>
        <dropDefaultValue tableName="t_data_views" columnName="last_trans_date" columnDataType="datetime"/>
</changeSet>
    
</databaseChangeLog>
