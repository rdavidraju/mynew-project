            ", assignType='" + assignType + "'" +
            ", assigneeId='" + assigneeId + "'" +
            ", email='" + email + "'" +
            ", autoApproval='" + autoApproval + "'" +
            ", createdBy='" + createdBy + "'" +
            ", creationDate='" + creationDate + "'" +
