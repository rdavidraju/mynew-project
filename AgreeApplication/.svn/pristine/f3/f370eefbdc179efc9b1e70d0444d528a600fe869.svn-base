    public List<BigInteger> convertLongToBig(List<Long> longIds)
    {
    	List<BigInteger> finalList = new ArrayList<BigInteger>();
    	if(longIds.size()>0)
    	{
    		for(Long id: longIds)
    		{
    			finalList.add(BigInteger.valueOf(id));
    		}
    	}
    	return finalList;
    }
    
