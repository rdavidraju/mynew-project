    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        SourceProfiles sourceProfiles = (SourceProfiles) o;
        if (sourceProfiles.getId() == null || getId() == null) {
            return false;
        }
        return Objects.equals(getId(), sourceProfiles.getId());
    }

