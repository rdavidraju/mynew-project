    <bpmn2:scriptTask id="_jbpm-unique-4" name="Set Owner" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <tns:metaData name="elementname">
          <tns:metaValue><![CDATA[Set Owner]]></tns:metaValue>
        </tns:metaData>
      </bpmn2:extensionElements>
