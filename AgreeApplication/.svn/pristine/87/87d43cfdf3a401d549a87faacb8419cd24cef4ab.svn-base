      <bpmn2:script>System.out.println(&quot;loopiteration&quot;+loopiteration);
System.out.println(&quot;owner: &quot;+list.get(loopiteration).toString());
//owner=list.get(0).toString();
	if (list.get(loopiteration) instanceof Map) {
			Map&lt;String, Object> map = (Map&lt;String, Object>) list.get(loopiteration);
			List list1 = new ArrayList();
			Iterator iterator = map.keySet().iterator();
			while (iterator.hasNext()) {
				Object key = iterator.next();
				Object value = map.get(key);
				list1 = (List) map.get(key);
				kcontext.setVariable(&quot;actiontype&quot;, key.toString());
				System.out.println(&quot;Script actiontype: &quot;+key.toString());
				System.out.println(&quot;Script owner: &quot;+list1.get(0).toString());
                             System.out.println(&quot;list1 sz: &quot;+list1.size());
				kcontext.setVariable(&quot;owner&quot;, list1.get(0).toString());
				if(list1.get(1) != null) {
                              System.out.println(&quot; list1.get(1).toString(): &quot;+ list1.get(1).toString());
					kcontext.setVariable(&quot;expirationTime&quot;, list1.get(1).toString());
				}
				else {
					kcontext.setVariable(&quot;expirationTime&quot;, &quot;365d&quot;);
				}
				if(list1.get(2) != null) {
                              System.out.println(&quot; list1.get(2).toString(): &quot;+ list1.get(2).toString());
					kcontext.setVariable(&quot;reassignTo&quot;, list1.get(2).toString());
				}
				else{
					kcontext.setVariable(&quot;reassignTo&quot;, &quot;noUser&quot;);
				}
				if(list1.get(3) != null) {
                              System.out.println(&quot; list1.get(3).toString(): &quot;+ list1.get(3).toString());
					kcontext.setVariable(&quot;ownerEmail&quot;, list1.get(3).toString());
				}
				else{
					kcontext.setVariable(&quot;ownerEmail&quot;, &quot;noEmail&quot;);
				}		
				System.out.println(&quot;Script owner: &quot;+list1.get(0).toString());
			}
		}
		System.out.println(&quot;loopiteration: &quot;+loopiteration);
		kcontext.setVariable(&quot;loopiteration&quot;, loopiteration+1);
		System.out.println(&quot;Approvers Count: &quot;+approverscount);</bpmn2:script>
    </bpmn2:scriptTask>
