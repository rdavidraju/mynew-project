		   log.info("Error while executing query: "+se);
       }
	   catch(Exception e)
	   {
		   log.info("Exception while getting databse properties");
	   }
	   finally{
	   		if(result != null)
	   			result.close();	
	   		if(stmt != null)
	   			stmt.close();
			if(conn != null)
				conn.close();
	   }
   		return viewOriginalIds;
   }

