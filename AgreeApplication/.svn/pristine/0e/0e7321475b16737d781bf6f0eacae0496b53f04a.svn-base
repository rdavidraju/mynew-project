					if(stmt != null)
						stmt.close();
					if(conn != null)
						conn.close();	    			
	    		}
	    		catch(Exception e)
	    		{
	    			log.info("Exception while closing jdbc statements.(Fetching recon source, target, rule ids) "+e);
	    		}    			
    		}
    		return finalList;
	    }
	    
