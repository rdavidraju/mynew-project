				while(result.next()){
					LinkedHashMap mp = new LinkedHashMap();
					for(int i=1; i<=colCount; i++)
     	    		{
						mp.put(rsmd.getColumnName(i).toString(), result.getString(i));
     	    		}
					finalList.add(mp);
				}
    		}
