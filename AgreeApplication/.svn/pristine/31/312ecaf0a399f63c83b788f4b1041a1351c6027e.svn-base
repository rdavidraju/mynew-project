package com.nspl.app.web.rest.dto;

import java.time.LocalDate;
import java.util.List;

public class Frequencies {
	private Long id;
	private String type;
	private Integer occurance;
	private LocalDate date;
	private String time;
	private List<String> day;
	private Long minutes;
	private Long hours;
	private List<String> month;
	private List<String> weekDay;
	
	private String oozieJobId;
	
	
	
	
	
	
	public String getOozieJobId() {
		return oozieJobId;
	}
	public void setOozieJobId(String oozieJobId) {
		this.oozieJobId = oozieJobId;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Integer getOccurance() {
		return occurance;
	}
	public void setOccurance(Integer occurance) {
		this.occurance = occurance;
	}
	public LocalDate getDate() {
		return date;
	}
	public void setDate(LocalDate date) {
		this.date = date;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public List<String> getDay() {
		return day;
	}
	public void setDay(List<String> day) {
		this.day = day;
	}
	public Long getMinutes() {
		return minutes;
	}
	public void setMinutes(Long minutes) {
		this.minutes = minutes;
	}
	public Long getHours() {
		return hours;
	}
	public void setHours(Long hours) {
		this.hours = hours;
	}
	public List<String> getMonth() {
		return month;
	}
	public void setMonth(List<String> month) {
		this.month = month;
	}
	public List<String> getWeekDay() {
		return weekDay;
	}
	public void setWeekDay(List<String> weekDay) {
		this.weekDay = weekDay;
	}
	
	
	
	

}
